@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>
@{
    string url = Request.Url.AbsolutePath;

    if (url.ToLower().IndexOf("/matrix") > -1)
    {
        ViewBag.Title = "Matrix Partial";
        Html.RenderAction("MatrixPartial", "Matrix", new { id = ViewBag.Id });
    }
    else if (url.ToLower().IndexOf("/installmatrix") > -1)
    {
        ViewBag.Title = "Install Matrix";
        Html.RenderAction("InstallMatrix", "Matrix", new { id = ViewBag.Id });
    }

}


