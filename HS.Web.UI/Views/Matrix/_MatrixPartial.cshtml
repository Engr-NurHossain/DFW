@{
    Layout = null;
}
@using Localize = HS.Web.UI.Helper.LanguageHelper 
@using AppConfig = HS.Framework.Utils.AppConfig
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
        $("#SalesMatrixTab").load("@(AppConfig.DomainSitePath)/Matrix/SalesMatrixPartial");
        @*$("#CommissionRangeTab").load("@(AppConfig.DomainSitePath)/Commission/CommissionRangePartial");*@
        @*$("#CommissionTab").load("@(AppConfig.DomainSitePath)/Commission/CommissionPartial");*@
    });
</script>
<style>
    .matrix-nav-tab > li.active{
        background-color: #2ca01c !important;
    }
    .matrix-nav-tab > li.active > a , .matrix-nav-tab > li.active > a:hover{
        color: #fff !important;
    }
    .matrix-nav-tab > li{
        border: 1px solid #ccc !important;
    }
    .matrix-nav-tab > li > a{
        background-color: transparent !important;
        border: none !important;
    }
</style>

<div class="container-fluid">
    <div class="matrix-content">
        <ul class="nav nav-tabs matrix-nav-tab" id="myTab">
            <li class="active"><a class="SalesMatrixTab" data-target="#SalesMatrixTab" data-toggle="tab">@Localize.T("Sales Matrix")</a></li>
            @*<li><a class="CommissionRange" data-target="#CommissionRangeTab" data-toggle="tab">@Localize.T("Commission Range")</a></li>*@
           
        </ul>
        <div class="tab-content">
            <div class="tab-pane active" id="SalesMatrixTab"></div>
            @*<div class="tab-pane" id="CommissionRangeTab"></div>*@
      
        </div>
    </div>
</div>
