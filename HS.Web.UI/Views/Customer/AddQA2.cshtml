        $("#btn_addccpaymentcustomerQA").click(function () {
        OpenRightToLeftModal("/SmartLeads/CCAddViewPaymentMethod?customerid=" + CustomerLoadGuid + "&type=Customer");
        });

        $("#SaveQA2").click(function () {
            if ($("#isCompleted").prop('checked') != true) {
                SaveQA2(false);
            }
            else {
                OpenErrorMessageNew("Error!", "This QA is completed", "");
