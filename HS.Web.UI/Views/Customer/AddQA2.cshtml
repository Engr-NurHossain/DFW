    var SaveQA2 = function (IsComplete) {
    var url = domainurl + "/Customer/AddQA2";
        var param = JSON.stringify({
            Id: $("#qa2Id").val(),
            CreatedBy:$("#createdBy").val(),
            CustomerId: CustomerLoadGuid,
            Street:$("#cusStreet").val(),
            City:$("#cusCity").val(),
            State:$("#cusState").val(),
            ZipCode: $("#cusZip").val(),
            AddressIsCorrect: $(".radBtnAddressIsCorrectYes").prop('checked')? "Yes" : $(".radBtnAddressIsCorrectNo").prop('checked')? "No" : "",
            PrimaryPhone: $("#cusPrimaryPhone").val(),
            PrimaryPhoneIsCorrect: $(".radBtnPhoneIsCorrectYes").prop('checked') ? "Yes" : $(".radBtnPhoneIsCorrectNo").prop('checked') ?"No":"",
            Passcode:$("#cusPasscode").val(),
            PasscodeIsCorrect:$(".radBtnPasscodeIsCorrectYes").prop('checked') ? "Yes" : $(".radBtnPasscodeIsCorrectNo").prop('checked')?"No":"",
            ContractTerm:$("#cusContractTerm").val(),
            MonitoringFee:$("#cusMonthFee").val(),
            TermAndFeeIsCorrect:$(".radBtnMonFeeAndConIsCorrectYes").prop('checked') ? "Yes" : $(".radBtnMonFeeAndConIsCorrectNo").prop('checked')?"No":"",
            IsReceiveCallOrText:$(".radBtnIsTextOrCallCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsTextOrCallCorrectNo").prop('checked')?"No":"",
            IsInstallThatPromised:$(".radBtnIsInstallCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsInstallCorrectNo").prop('checked')?"No":"",
            IsShowSystem:$(".radBtnIsShowSystemCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsShowSystemCorrectNo").prop('checked')?"No":"",
            IsCleanUpAfterInstallation:$(".radBtnIsCleanCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsCleanCorrectNo").prop('checked')?"No":"",
            ExperienceRate: $(".radBtnExperienceRate1").prop('checked') ? "1"
                : $(".radBtnExperienceRate2").prop('checked') ? "2"
                : $(".radBtnExperienceRate3").prop('checked') ? "3"
                    : $(".radBtnExperienceRate4").prop('checked') ? "4"
                        : $(".radBtnExperienceRate5").prop('checked') ? "5" : "",
            ManualNote: $("#manualNote").val(),
            CompletedBy: $("#CompletedBy").val(),
            GallantFew: $(".radBtnGallantFewYes").prop('checked') ? "Yes" : "",
            TextLinkForTremendously: $(".radBtnTextLinkForTremendouslyYes").prop('checked') ? "Yes" : "",
            AccountOnline: $(".radBtnAccountOnlineYes").prop('checked') ? "Yes" : "",
            AgreementSigned: $(".radBtnAgreementSignedYes").prop('checked') ? "Yes" : "",
            FirstMonthSetup: $(".radBtnFirstMonthSetupYes").prop('checked') ? "Yes" : "",
            IsCompletelySatisfied: $(".radBtnIsSatisfiedCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsSatisfiedCorrectNo").prop('checked') ? "No" : $(".radBtnIsSatisfiedCorrectNA").prop('checked') ? "NA" : "",
            FinanceCompletelySatisfied:$(".radBtnFinanceCompletelySatisfiedYes").prop('checked') ? "Yes" : $(".radBtnFinanceCompletelySatisfiedNo").prop('checked') ? "No" : $(".radBtnFinanceCompletelySatisfiedNA").prop('checked') ? "NA" :"",
            DiscussionIsOkay:$(".radBtnIsDiscussCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsDiscussCorrectNo").prop('checked')?"No":"",
            IsCompleted:IsComplete
    })
    $.ajax({
        type: "POST",
        ajaxStart: $(".loader-div").show(),
        url: url,
        data: param,
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        cache: false,
        success: function (data) {
            if (data.result == true) {
                OpenSuccessMessageNew("Success!", "QA2 saved successfully", function () {
                    CloseTopToBottomModal();
                    QA2Load();
                });
            }
            else {
                OpenErrorMessageNew('Error!', "QA2 saved failed", "");
            }
        },
        error: function (jqXHR, textStatus, errorThrown) {
            console.log(errorThrown);
        }
    });
}
    $(document).ready(function () {
        $(".qa_inner_height").height(window.innerHeight - 86);
