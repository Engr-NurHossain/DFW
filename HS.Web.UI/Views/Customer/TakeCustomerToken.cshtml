@{
    Layout = null;
}
@model HS.Entities.VehicleRepairLog


@using Localize = HS.Web.UI.Helper.LanguageHelper
@Styles.Render("~/styles/AddFile")
<script src="~/Content/JQuery/jquery-3.1.1.js"></script>
<link href="~/Content/MagnificPopUp/magnific-popup.css" rel="stylesheet" />
<script src="~/Content/Js/Layout/utils.js"></script>
<script src="~/Content/MagnificPopUp/jquery.magnific-popup.js"></script>
<script>
   
    $(document).ready(function () {
        var idlist = [{ id: ".ClockTokenPopUp", type: 'iframe', width: 500, height: 500 }];
        jQuery.each(idlist, function (i, val) {
            magnificPopupObj(val);
        });
    
      
    })
</script>



<div class="add_file_header">@Localize.T("Put Your OTP Here")</div>
<div class="container_addfile add_vehicle_repair_container">

  
 

    <span class="content-type">@Localize.T("Amount")</span>
    <div class="txt_form_add_file">
        <input type="number" class="form-control" id="Amount" value="" name="Amount" placeholder="@Localize.T("Quick Book No")" datarequired="true" />
        <label rel="Amount" class="label hidden red position-absolute">*@Localize.T("Amount Required")</label>
    </div>



   

</div>
<section class="footer-section">
    <footer class="footer">
        <div class="SaveCancelDiv">
            <button class="btn btn_file" id="btnSaveHrVehicleRepair"><i class="fa fa-floppy-o" aria-hidden="true"></i>@Localize.T(" Save")</button>

        </div>
    </footer>
</section>

