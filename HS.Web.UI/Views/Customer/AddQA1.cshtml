@model HS.Entities.CreateQA1Model

@{
    Layout = null;
    string Currency = HS.Web.UI.Helper.LabelHelper.CurrentTransMakeCurrency.MakeCurrency(null);
}
@using Localize = HS.Web.UI.Helper.LanguageHelper
@using AppConfig = HS.Framework.Utils.AppConfig;
@using PermissionChekcer = HS.Web.UI.Helper.PermissionHelper
@using Permissions = HS.Framework.UserPermissions
<link href="~/Content/Css/Lead/EmergencyContactListPartial.css" rel="stylesheet" />
<link href="~/Content/Css/Customer/AddQA.css" rel="stylesheet" />
<script src="~/Content/PikDay/js/pikaday.js"></script>
<script>
    var DateOfBirthDatePicker;
    var InstallDatePicker;
    var OpenQA1TicketById = function (ticketId) {
        SaveQA1(false);
        OpenTopToBottomModal("@(AppConfig.DomainSitePath)/CustomerPublic/AddTicket/?Id=" + ticketId);
    }
    var SaveQA1 = function (IsComplete) {
    var url = domainurl + "/Customer/AddQA1";
        var param = JSON.stringify({
            Id: $("#qa1Id").val(),
            CreatedBy:$("#createdBy").val(),
            CustomerId: CustomerLoadGuid,
            FirstName: $("#cusFirstName").val(),
            LastName: $("#cusLastName").val(),
            SSN:$("#txtSSN").val(),
            HowQA1Initiated: $(".radBtnHowQA1InitiatedSkype").prop('checked') ? "Skype"
                : $(".radBtnHowQA1InitiatedRepCallIn").prop('checked') ? "RepCallIn"
                : $(".radBtnHowQA1InitiatedTechCallIn").prop('checked') ? "TechCallIn"
                    : $(".radBtnHowQA1InitiatedAlreadyScheduledOutbound").prop('checked') ? "AlreadyScheduledOutbound"
                        : $(".radBtnHowQA1InitiatedAlreadyScheduledInbound").prop('checked') ? "AlreadyScheduledInbound" : "",
            BillingIsCorrect: $(".radBtnBillingIsCorrectConfirmed").prop('checked') ? "Confirmed"
                : $(".radBtnBillingIsCorrectACH").prop('checked') ? "ACH"
                : $(".radBtnBillingIsCorrectCC").prop('checked') ? "CC"
                        : $(".radBtnBillingIsCorrectWillProvide").prop('checked') ? "WillProvide" : "",
            PermissionGoAhead: $(".radBtnPermissionGoAheadConfirmed").prop('checked') ? "Confirmed"
                : $(".radBtnPermissionGoAheadUpdated").prop('checked') ? "Updated" : "",
            SoundIsCorrect: $(".radBtnSoundIsCorrectConfirmed").prop('checked') ? "Confirmed"
                : $(".radBtnSoundIsCorrectMissing").prop('checked') ? "Missing" : "",
            PermissionGoAheadNote: $("#PermissionGoAheadNote").val(),
            SoundCorrectNote: $("#SoundCorrectNote").val(),
            NameIsCorrect: $(".radBtnNameIsCorrectYes").prop('checked') ? "Yes" : $(".radBtnNameIsCorrectNo").prop('checked') ? "No" : "",
            SSNIsCorrect: $(".radBtnIsFourDigitSSNYes").prop('checked') ? "Yes" : $(".radBtnIsFourDigitSSNNo").prop('checked') ? "No" : "",
            IsCallingToday: $(".radBtnIsCallingTodayYes").prop('checked')? "Yes" : $(".radBtnIsCallingTodayNo").prop('checked')? "No" : "",
            Street: $("#cusStreet").val(),
            City:$("#cusCity").val(),
            State:$("#cusState").val(),
            ZipCode: $("#cusZip").val(),
            AddressIsCorrect: $(".radBtnAddressIsCorrectYes").prop('checked')? "Yes" : $(".radBtnAddressIsCorrectNo").prop('checked')? "No" : "",
            IsLocationHome: $(".radBtnIsHomeLocationCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsHomeLocationCorrectNo").prop('checked') ? "No" : "",
            IsHomeRent: $(".radBtnIsHomeRentCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsHomeRentCorrectNo").prop('checked') ? "No" : "",
            HomeownerName: $("#homeownerName").val(),
            IsSpeckRep: $(".radBtnIsSpeckRepCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsSpeckRepCorrectNo").prop('checked') ? "No" : "",
            IsHomeownerAuthorized: $(".radBtnIsHomeownerAuthorizedCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsHomeownerAuthorizedCorrectNo").prop('checked') ? "No" : "",
            IsOnTheDeed: $(".radBtnIsOnTheDeedCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsOnTheDeedCorrectNo").prop('checked') ? "No" : "",
            IsResponsibleParty: $(".radBtnIsResponsiblePartyCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsResponsiblePartyCorrectNo").prop('checked') ? "No" : "",
            ResponsiblePartyName: $("#responsiblePartyName").val(),
            IsSignShowingBusiness: $(".radBtnIsSignShowingBusinessCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsSignShowingBusinessCorrectNo").prop('checked') ? "No" : "",
            SignShowText: $("#signShowText").val(),
            IsOtherDocumentShowing:$(".radBtnIsOtherDocumentShowingCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsOtherDocumentShowingCorrectNo").prop('checked') ? "No" : "",
            OtherDocumentShowingText: $("#otherDocumentShowingText").val(),
            IsBusinessSecretaryOfState: $(".radBtnIsBusinessSecretaryOfStateCorrectYes").prop('checked') ? "Yes" : $(".radBtnIsBusinessSecretaryOfStateCorrectNo").prop('checked') ? "No" : "",
            PrimaryPhone: $("#cusPrimaryPhone").val(),
            PrimaryPhoneIsCorrect: $(".radBtnPhoneIsCorrectYes").prop('checked') ? "Yes" : $(".radBtnPhoneIsCorrectNo").prop('checked') ?"No":"",
            EmailAddress: $("#cusEmailAddress").val(),
            EmailIsCorrect: $(".radBtnEmailIsCorrectYes").prop('checked') ? "Yes" : $(".radBtnEmailIsCorrectNo").prop('checked') ? "No" : "",
            InstallTicketIsCorrect: $(".radBtnInstallTicketIsCorrectYes").prop('checked') ? "Yes" : $(".radBtnInstallTicketIsCorrectNo").prop('checked') ? "No" : "",
            TermAndFeeIsCorrect: $(".radBtnMonFeeAndConIsCorrectYes").prop('checked') ? "Yes" : $(".radBtnMonFeeAndConIsCorrectNo").prop('checked') ? "No" : "",
            IsCurrentlyInContract: $(".radBtnIsCurrentlyInContractYes").prop('checked') ? "Yes" : $(".radBtnIsCurrentlyInContractNo").prop('checked') ? "No" : "",
            CurrentlyContractText: $("#currentlyContractText").val(),
            IsUnderstandAffiliateAndService: $(".radBtnIsUnderstandAffiliateAndServiceYes").prop('checked') ? "Yes" : $(".radBtnIsUnderstandAffiliateAndServiceNo").prop('checked') ? "No" : "",
            UnderstandText: $("#understandText").val(),
            IsBrinksOrMonitronic: $(".radBtnIsBrinksOrMonitronicYes").prop('checked') ? "Yes" : $(".radBtnIsBrinksOrMonitronicNo").prop('checked') ? "No" : "",
            BrinksOrMonitronicText: $("#brinksOrMonitronicText").val(),
            BrinksHowLong: $("#BrinksHowLong").val(),
            IsPromisedByRep: $(".radBtnIsPromisedByRepYes").prop('checked') ? "Yes" : $(".radBtnIsPromisedByRepNo").prop('checked') ? "No" : "",
            PromisedByRepText: $("#promisedByRepText").val(),
            WhatWasPromised: $("#whatWasPromised").val(),
            IsElectricityAvailable: $(".radBtnIsElectricityAvailableYes").prop('checked') ? "Yes" : $(".radBtnIsElectricityAvailableNo").prop('checked') ? "No" : "",
            IsCorrectlyWWI: $(".radBtnIsCorrectlyWWIYes").prop('checked') ? "Yes" : $(".radBtnIsCorrectlyWWINo").prop('checked') ? "No" : "",
            IsCorrectlyWWINoText: $(".radBtnIsCorrectlyWWINoTextYes").prop('checked') ? "Yes" : $(".radBtnIsCorrectlyWWINoTextNo").prop('checked') ? "No" : "",
            IsCorrectlyWWIYesText: $(".radBtnIsCorrectlyWWIYesTextYes").prop('checked') ? "Yes" : $(".radBtnIsCorrectlyWWIYesTextNo").prop('checked') ? "No" : "",
            TicketScheduleTimeIsGood: $(".radBtnTicketScheduleTimeIsGoodYes").prop('checked') ? "Yes" : $(".radBtnTicketScheduleTimeIsGoodNo").prop('checked') ? "No" : "",
            DateOfBirthIsCorrect: $(".radBtnDateOfBirthIsCorrectYes").prop('checked') ? "Yes" : $(".radBtnDateOfBirthIsCorrectNo").prop('checked') ? "No" : "",
            DateofBirth: $("#dateofBirth").val(),
            InstallDate: $("#installDate").val(),
            QARep: $("#QA1Rep").val(),
            LastUpdatedByUid: $("#LastUpdatedByUid").val(),
            SalesRep: $("#SalesRep").val(),
            IsGenerallyResponsible: $(".radBtnIsGenerallyResponsibleYes").prop('checked') ? "Yes" : $(".radBtnIsGenerallyResponsibleNo").prop('checked') ? "No" : "",
            IsPass: $(".radBtnIsPassPASS").prop('checked') ? "PASS" : $(".radBtnIsPassFAIL").prop('checked') ? "FAIL" : "",
            PassNote: $("#PassNote").val(),
            PermitIsRequired: $(".radBtnPermitIsRequiredYes").prop('checked') ? "Yes" : "",
            IsAssistYou: $(".radBtnIsAssistYouYes").prop('checked') ? "Yes" : "",
            Passcode: $("#cusPasscode").val(),
            ManualNote:$("#manualNote").val(),
            IsSignShowingBusinessYes: $(".radBtnIsSignShowingBusinessYesYes").prop('checked') ? "Yes" : $(".radBtnIsSignShowingBusinessYesNo").prop('checked') ? "No" : "",
            IsCompleted:IsComplete
    })
    $.ajax({
        type: "POST",
        ajaxStart: $(".loader-div").show(),
        url: url,
        data: param,
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        cache: false,
        success: function (data) {
            if (data.result == true) {
                OpenSuccessMessageNew("Success!", "QA1 saved successfully", function () {
                    CloseTopToBottomModal();
                    QA1Load();
                });
            }
            else {
                OpenErrorMessageNew('Error!', "QA1 saved failed", "");
            }
        },
        error: function (jqXHR, textStatus, errorThrown) {
            console.log(errorThrown);
        }
    });
    }
    var QA1LoadAddNewEmergencyContactDiv = function () {
    $("#QA1AddNewEmgContactPartial").show();
    $("#QA1AddNewEmgContactPartial").load(domainurl + "/Customer/AddCustomerEmergencyContact?EmgConId=" + CustomerLoadGuid + "&EmgId=0");
}
    var LoadQA1EmergencyContactList = function () {
    $("#QA1EmergencyContactList").load(domainurl + "/Customer/CustomerEmergencyContactListPartial?Id=" + CustomerLoadGuid);
    }
    var HideNewEmgContactPartial = function () {
    $("#QA1AddNewEmgContactPartial").hide();
    }
    var EditEmergencyContactDiv = function (emgId) {
    $("#QA1AddNewEmgContactPartial").show();
    $("#QA1AddNewEmgContactPartial").load(domainurl + "/Customer/AddCustomerEmergencyContact?EmgConId=" + CustomerLoadGuid + "&EmgId=" + emgId);
}
    $(document).ready(function () {
        $(".qa_inner_height").height(window.innerHeight - 86);
        DateOfBirthDatePicker = new Pikaday({
            format: 'MM/DD/YY',
            field: $('#dateofBirth')[0],
            trigger: $('#DateofBirth_custom')[0],
            firstDay: 1
        });
        InstallDatePicker = new Pikaday({
            format: 'MM/DD/YY',
            field: $('#installDate')[0],
            trigger: $('#installDate_custom')[0],
            firstDay: 1
        });
        $(".radBtnNameIsCorrect_cls").change(function () {
            if ($(".radBtnNameIsCorrect_cls").prop('checked')) {
                $(".add_name").attr('style','pointer-events:none;');
            }
            else {
               $(".add_name").attr('style','pointer-events:auto;');
            }
        });
        $(".radBtnIsFourDigitSSN_cls").change(function () {
            if ($(".radBtnIsFourDigitSSN_cls").prop('checked')) {
                $(".add_ssn").attr('style','pointer-events:none;');
            }
            else {
               $(".add_ssn").attr('style','pointer-events:auto;');
            }
        });

        $(".radBtnIsHomeLocationCorrect_cls").change(function () {
            if ($(".radBtnIsHomeLocationCorrect_cls").prop('checked')) {
                $(".location_home").show();
                $(".location_business").hide();
            }
            else {
               $(".location_home").hide();
                $(".location_business").show();
            }
        });
        $(".radBtnIsHomeRentCorrect_cls").change(function () {
            if ($(".radBtnIsHomeRentCorrect_cls").prop('checked')) {
                $(".rent_home").show();
                $(".own_home").hide();
            }
            else {
               $(".rent_home").hide();
                $(".own_home").show();
            }
        });
        $(".radBtnIsResponsiblePartyCorrect_cls").change(function () {
            if ($(".radBtnIsResponsiblePartyCorrect_cls").prop('checked')) {
                $(".responsible_party_name_div").hide();
            }
            else {
                $(".responsible_party_name_div").show();
            }
        });
        $(".radBtnIsSignShowingBusinessCorrect_cls").change(function () {
            if ($(".radBtnIsSignShowingBusinessCorrect_cls").prop('checked')) {
                $(".sign_show_yes_div").show();
                $(".sign_show_no_div").hide();
            }
            else {
                $(".sign_show_yes_div").hide();
                $(".sign_show_no_div").show();
            }
        });

        $(".radBtnIsCurrentlyInContract_cls").change(function () {
            if ($(".radBtnIsCurrentlyInContract_cls").prop('checked')) {
                $(".currently_contract_div").hide();
                $(".understant_div").show();
            }
            else {
                $(".currently_contract_div").show();
                $(".understant_div").hide();
            }
        });
        $(".radBtnIsUnderstandAffiliateAndService_cls").change(function () {
            if ($(".radBtnIsUnderstandAffiliateAndService_cls").prop('checked')) {
                $(".understant_text_div").hide();
            }
            else {
                $(".understant_text_div").show();
            }
        });
        $(".radBtnIsBrinksOrMonitronic_cls").change(function () {
            if ($(".radBtnIsBrinksOrMonitronic_cls").prop('checked')) {
                $(".brink_or_monitronic_div").hide();
                $(".brink_or_monitronic_yes_div").show();
            }
            else {
                $(".brink_or_monitronic_div").show();
                $(".brink_or_monitronic_yes_div").hide();
            }
        });

        $(".radBtnIsPromisedByRep_cls").change(function () {
            if ($(".radBtnIsPromisedByRep_cls").prop('checked')) {
                $(".promised_rep_div").hide();
                $(".promised_rep_yes_div").show();
            }
            else {
                $(".promised_rep_div").show();
                $(".promised_rep_yes_div").hide();
            }
        });
        $(".radBtnIsCorrectlyWWI_cls").change(function () {
            if ($(".radBtnIsCorrectlyWWI_cls").prop('checked')) {
                $(".correctly_wwi_no").hide();
                $(".correctly_wwi_yes").show();
            }
            else {
                $(".correctly_wwi_no").show();
                $(".correctly_wwi_yes").hide();
            }
        });
        $(".radBtnAddressIsCorrect_cls").change(function () {
            if ($(".radBtnAddressIsCorrect_cls").prop('checked')) {
                $(".add_address").attr('style','pointer-events:none;');
            }
            else {
               $(".add_address").attr('style','pointer-events:auto;');
            }
        });
        $(".radBtnPhoneIsCorrect_cls").change(function () {
            if ($(".radBtnPhoneIsCorrect_cls").prop('checked')) {
                $("#cusPrimaryPhone").attr('disabled','disabled');
            }
            else {
               $("#cusPrimaryPhone").removeAttr('disabled');
            }
        });
        $(".radBtnEmailIsCorrect_cls").change(function () {
            if ($(".radBtnEmailIsCorrect_cls").prop('checked')) {
                $("#cusEmailAddress").attr('disabled','disabled');
            }
            else {
               $("#cusEmailAddress").removeAttr('disabled');
            }
        });
        $(".radBtnDateOfBirthIsCorrect_cls").change(function () {
            if ($(".radBtnDateOfBirthIsCorrect_cls").prop('checked')) {
                //$("#dateofBirth").attr('disabled', 'disabled');
                $(".dateOfBirth_add").attr('style','pointer-events:none;');
            }
            else {
                //$("#dateofBirth").removeAttr('disabled');
                $(".dateOfBirth_add").attr('style','pointer-events:auto;');
            }
        });

        LoadQA1EmergencyContactList();
    $("#btnQA1CustomerAddNewEmgContact").click(function () {
        QA1LoadAddNewEmergencyContactDiv();
    });
        $("#SaveQA1").click(function () {
            if ($("#isCompleted").prop('checked') != true) {
                SaveQA1(false);
            }
            else {
                OpenErrorMessageNew("Error!", "This QA is completed", "");
            }
        });
        $("#FinishQA1").click(function () {
            if ($("#isCompleted").prop('checked') != true) {
                OpenConfirmationMessageNew("Confirm?", "Are you sure you want to complete this QA?", function () {
                 SaveQA1(true);
            });

            }
            else {
                OpenErrorMessageNew("Error!", "This QA is completed", "");
            }
    });
    });
    $(window).resize(function () {
        $(".qa_inner_height").height(window.innerHeight - 86);
    })
</script>

<div class="qa_container">
    <div class="qa_header">
        QA1 Script
    </div>
    <div class="qa_inner_height">
        <input hidden id="qa1Id" value="@(Model.qa1Script != null && Model.qa1Script.Id > 0 ? Model.qa1Script.Id : 0)" />
        <input hidden id="createdBy" value="@(Model.qa1Script != null && !string.IsNullOrWhiteSpace(Model.qa1Script.CreatedBy) ? Model.qa1Script.CreatedBy : Model.UserName)" />
        <input hidden id="isCompleted" type="checkbox" @(Model.qa1Script != null && Model.qa1Script.IsCompleted ? "checked=\"checked\"" : "") value="true" />

        <div class="qa_inner">
            <div class="qa_inner_content">
                <ol>
                    <li>
                        QA Rep Name<span class="qa_red_color">*</span>
                        <div class="width_100p">
                            <div class="input-group add-on">
                                <select class="form-control" id="QA1Rep">
                                    @if (ViewBag.CurrentEmployeeList != null)
                                    {
                                        foreach (var Item in (List<HS.Entities.Employee>)ViewBag.CurrentEmployeeList)
                                        {
                                            <option @(Model.qa1Script != null && Model.qa1Script.QARep.ToString() != "00000000-0000-0000-0000-000000000000" && Model.qa1Script.QARep.ToString() == Item.UserId.ToString() ? "selected" : (ViewBag.CurrentUserId == Item.UserId.ToString() ? "selected" : "")) value="@Item.UserId">@Item.FirstName @Item.LastName</option>
                                        }
                                    }
                                </select>
                            </div>
                        </div>
                    </li>
                    <li>
                        Sales Rep Name<span class="qa_red_color">*</span>
                        <div class="width_100p">
                            <div class="input-group add-on">
                                <select class="form-control" id="SalesRep">
                                    @if (ViewBag.CurrentEmployeeList != null)
                                    {
                                        foreach (var Item in (List<HS.Entities.Employee>)ViewBag.CurrentEmployeeList)
                                        {
                                            <option @(Model.qa1Script != null && Model.qa1Script.SalesRep.ToString() != "00000000-0000-0000-0000-000000000000" && Model.qa1Script.SalesRep.ToString() == Item.UserId.ToString() ? "selected" : (Model.customer != null && Model.customer.Soldby.ToString() == Item.UserId.ToString() ? "selected" : "")) value="@Item.UserId">@Item.FirstName @Item.LastName</option>
                                        }
                                    }
                                </select>
                            </div>
                        </div>
                    </li>
                    <li>
                        Customer Name<span class="qa_red_color">*</span>
                        <div class="width_100p">
                            <div class="input-group add-on">
                                <input type="text" class="form-control" disabled="disabled" value="@(Model.customer != null && (!string.IsNullOrWhiteSpace(Model.customer.FirstName)||!string.IsNullOrWhiteSpace(Model.customer.LastName)) ? Model.customer.FirstName + " " + Model.customer.LastName : "")" />
                            </div>
                        </div>
                    </li>
                    <li>
                        Install Date <span class="qa_red_color">*</span>
                        <div class="width_100p">
                            <div class="input_group_for_date add-on installDate_add" id="installDate_custom" style="pointer-events:auto;">
                                <input type="text" id="installDate" value="@(Model.qa1Script != null && Model.qa1Script.InstallDate!=new DateTime() && Model.qa1Script.InstallDate!=null ? Model.qa1Script.InstallDate.ToString("MM/dd/yy") : DateTime.Now.ToString("MM/dd/yy"))" class="form-control" placeholder="Install Date">
                                <div class="input-group-btn">
                                    <button class="btn installDate_Calender">
                                        <i class="fa fa-calendar" aria-hidden="true"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li>
                        How was QA initiated? <span class="qa_red_color">*</span>
                        @{
                            if (Model.qa1Script != null && !string.IsNullOrWhiteSpace(Model.qa1Script.HowQA1Initiated) && Model.qa1Script.HowQA1Initiated == "Skype")
                            {
                                <div class="width_100p">
                                    <label class="qa_black_check predefine_check">@Html.RadioButton("radBtnHowQA1Initiated", true, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedSkype", @checked = "checked" })@Localize.T("Skype")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedRepCallIn" })@Localize.T("Rep call in")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedTechCallIn" })@Localize.T("Tech call in")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedAlreadyScheduledOutbound" })@Localize.T("Already scheduled (outbound)")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedAlreadyScheduledInbound" })@Localize.T("Already scheduled (inbound)")</label>
                                </div>
                            }
                            else if (Model.qa1Script != null && !string.IsNullOrWhiteSpace(Model.qa1Script.HowQA1Initiated) && Model.qa1Script.HowQA1Initiated == "RepCallIn")
                            {
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", true, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedSkype" })@Localize.T("Skype")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check predefine_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedRepCallIn", @checked = "checked" })@Localize.T("Rep call in")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedTechCallIn" })@Localize.T("Tech call in")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedAlreadyScheduledOutbound" })@Localize.T("Already scheduled (outbound)")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedAlreadyScheduledInbound" })@Localize.T("Already scheduled (inbound)")</label>
                                </div>
                            }
                            else if (Model.qa1Script != null && !string.IsNullOrWhiteSpace(Model.qa1Script.HowQA1Initiated) && Model.qa1Script.HowQA1Initiated == "TechCallIn")
                            {
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", true, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedSkype" })@Localize.T("Skype")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedRepCallIn" })@Localize.T("Rep call in")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check predefine_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedTechCallIn", @checked = "checked" })@Localize.T("Tech call in")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedAlreadyScheduledOutbound" })@Localize.T("Already scheduled (outbound)")</label>
                                </div>
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", false, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedAlreadyScheduledInbound" })@Localize.T("Already scheduled (inbound)")</label>
                                </div>
                            }
                            else if (Model.qa1Script != null && !string.IsNullOrWhiteSpace(Model.qa1Script.HowQA1Initiated) && Model.qa1Script.HowQA1Initiated == "AlreadyScheduledOutbound")
                            {
                                <div class="width_100p">
                                    <label class="qa_black_check">@Html.RadioButton("radBtnHowQA1Initiated", true, new { @class = "radBtnHowQA1Initiated_cls radBtnHowQA1InitiatedSkype" })@Localize.T("Skype")</label>
