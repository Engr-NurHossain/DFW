                        <button class="btn btn-success Active" id="ResendCancellationFile" idval="@Model.Id"> @Localize.T("Resend Cancellation File")</button>
                    }
                    if (PermissionChekcer.IsPermitted(Permissions.CustomerPermissions.TransferCustomer))
                    {
                        <button class="btn btn-success Active" id="" onclick="TransferLeadConfirm('@Model.CustomerId.ToString()')">@Localize.T(" Transfer")</button>
                    }
                }
                else
                {
                    if (PermissionChekcer.IsPermitted(Permissions.CustomerPermissions.ActivateCustomerPermission))
                    {
                        <button class="btn btn-InorActive Active" id="InorActivebtn" idval="@Model.Id"><i class="fa fa-check" aria-hidden="true"></i>@Localize.T(" Activate")</button>
                    }

                }
            }
            <button onclick="CloseTopToBottomModal()" class="btn btn-danger acButton_for_desktop" id="Cancel"><i class="fa fa-times" aria-hidden="true"></i> @Localize.T("Close")</button> @*This button shows on Desktop*@
            <button onclick="CloseTopToBottomModal()" class="btn btn-danger acButton_for_mobile" id="Cancel"><i class="fa fa-times" aria-hidden="true"></i></button> @*This button shows on Device*@
            @if (PermissionChekcer.IsPermitted(Permissions.CustomerPermissions.CustomerDelete))
            {
                <button class="btn btn-default DeleteThisCustomer acButton_for_desktop" id="DeleteThisCustomer"><i class="fa fa-trash-o" aria-hidden="true"></i> @Localize.T("Delete")</button> @*This button shows on Desktop*@
                <button class="btn btn-default DeleteThisCustomer acButton_for_mobile" id="DeleteThisCustomer"><i class="fa fa-trash-o" aria-hidden="true"></i></button> @*This button shows on Device*@
            }
        </div>
    </footer>
</section>
