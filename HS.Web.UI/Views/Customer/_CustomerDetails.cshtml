    List<HS.Entities.GridSetting> CustomerBlockUiSetting = (List<HS.Entities.GridSetting>)ViewBag.CustomerBlockUiSetting;
    List<HS.Entities.GridSetting> CustomerTabUiSetting = (List<HS.Entities.GridSetting>)ViewBag.CustomerTabUiSetting;
    List<HS.Entities.GridSetting> CustomerDetailBlock = ((List<HS.Entities.GridSetting>)ViewBag.CustomerDetailBlock).Where(x => x.FormActive == true).ToList();
    //bool QA2ActiveLink = false;
    //bool TechCall = false;
    bool IsDFW = false;
    string IsDFWS = ConfigurationManager.AppSettings["IsDFW"];
    if (IsDFWS == "true")
    {
        IsDFW = true;
    }
    double ContractTerm = 0;
    var LeadSmartSetup = CustomerGridSetting.Where(x => x.SelectedColumn == "LeadSmartSetUp");
    var CurrentUser = (HS.Web.UI.Helper.CustomPrincipal)User;
    string Currency = HS.Web.UI.Helper.LabelHelper.CurrentTransMakeCurrency.MakeCurrency(CurrentUser.CompanyId.Value);
