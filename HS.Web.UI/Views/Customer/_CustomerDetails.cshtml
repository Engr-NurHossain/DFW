<script src="~/Content/Js/Note/Note.js"></script>
<script>
    //var CustomerGuid = '@Model.CustomerId';
    var customernoteid = '@ViewBag.CustomerNoteList';
    var ListQA = '@ViewBag.ObjQa';
    var tab = '@ViewBag.tab';
    var DestinationCusAddress = '@Address';
    var TaxExemption = '@Model.TaxExemption';
    var EnvelopeID = '@Model.EcontractEnvlobeId';
    var RMROverviewHistory = '@(Model.RMR != null ? true : false)';
    var CustomerName = '@(Model.Type == "Commercial" && Model.BusinessName != ""? Model.BusinessName:string.Format("{0} {1}", Model.FirstName, Model.LastName))';
    var TabsLoaderText = "<div class='invoice-loader'><div><div class='lds-css ng-scope'><div style='margin:auto; z-index:99;' class='lds-double-ring'><div></div><div></div></div></div></div></div>";
    var OpenQA1Call = function () {
        OpenTopToBottomModal(domainurl + "/Customer/QA1QuestionariePartial?id=" + CustomerLoadId);
    }
    var OpenQA2Call = function () {
        OpenTopToBottomModal(domainurl + "/Customer/QA2QuestionariePartial?id=" + CustomerLoadId);
    }
    var OpenInvFromCustomerById = function (invId) {
        if (typeof (invId) != "undefined" && invId > 0) {
            OpenTopToBottomModal(domainurl + "/Invoice/AddInvoiceCustomer/?Id=" + invId);
        }
    }
    var AddOpportunity = function (CustomerId) {
        OpenTopToBottomModal(domainurl + "/Opportunity/AddOpportunity?CustomerId=" + CustomerId);
    }
    var AddAdditionalContact = function () {
        OpenRightToLeftModal(domainurl + "/Customer/CustomerAdditionalInfo?CustomerId=" + CustomerLoadGuid);
    }
    var AddSystemInformation = function () {
        //OpenRightToLeftModal(domainurl + "/Customer/CustomerAdditionalInfo?CustomerId=" + CustomerLoadGuid);
    }
    var OpenTechcall = function () {
        OpenTopToBottomModal(domainurl + "/Customer/LeadTechCallPartial?id=" + CustomerLoadId);
    }
    var ShowTag = function (item) {
        $("#isVerifyEmail").attr("data-original-title", "");
        $("#isVerifyEmail").attr("data-original-title", item);
    }

    var ShowEquipmentDetails = function (EqpId) {
        OpenTopToBottomModal(domainurl + "/Customer/CustomerEquipmentDetailsPopUp?customerid=" + CustomerLoadGuid + "&EqpId=" + EqpId);
    }
    var HistoryLoad = function (pageNo, order) {
        if (typeof (pageNo) != "undefined" && pageNo > 0) {
            var LoadUrl = domainurl + "/Customer/HistoryLogPartial/?CustomerId=" + CustomerLoadGuid + "&PageNo=" + pageNo + "&order=" + order + "&pagesize=10";
            $(".HistoryLog").html(TabsLoaderText);
            $(".HistoryLog").load(LoadUrl);
        }
    }
    var RMROverviewLoad = function (pageNo, order) {
        if (typeof (pageNo) != "undefined" && pageNo > 0) {
            var LoadUrl = domainurl + "/Customer/RMROverview/?CustomerId=" + CustomerLoadGuid;
            $(".RMROverview").html(TabsLoaderText);
            $(".RMROverview").load(LoadUrl);
        }
    }

    $(".btnSmartLeadSetup").click(function () {
        var valid = $(this).attr('idval');
        LoadSmartLeadSetup(valid, true)
    })
    $(".btnLeadSetup").click(function () {
        var valid = $(this).attr('idval');
        LoadLeadSetup(valid, true)
    })
    $(document).ready(function () {
        document.title = CustomerName + "'s Profile";
        var LoadCustomerDiv = "#customer_tab_" + CustomerLoadId + " ";

        $('ul.customer_detail_tab_list li a').click(function (e) {
            if (!TabPopStateCheck) {
                window.history.pushState({ urlPath: window.location.pathname }, "", $(e.target).attr('tabname'));
            }
            TabPopStateCheck = false;
        });
        HistoryLoad(1, null);
        if (RMROverviewHistory == "True") {
            RMROverviewLoad(1, null);
        }
        if (top.location.hash != '') {
            if ($("[tabname='" + top.location.hash + "']").length > 0) {

                TabPopStateCheck = true;
                $("[tabname='" + top.location.hash + "']").click();
            }
        }

        if (tab != "" && tab == "Reminder") {
            OpenNotesTab();
            OpenRightToLeftModal(domainurl + "/Customer/AddNewReminderNote?id=" + '@ViewBag.noteid' + "&CustomerId=" + CustomerLoadGuid + "&Timeval=" + '@ViewBag.time' + "&IsComplete=" + '@ViewBag.complete');
        }
        if (tab != "" && tab == "Note") {
            OpenNotesTab();
            OpenRightToLeftModal(domainurl + "/Notes/AddNotes?id=" + '@ViewBag.noteid' + "&customerid=" + CustomerLoadId);
        }

        $(LoadCustomerDiv + "#SystemInfo").load(domainurl + "/Leads/LeadSystemInfo/?CustomerId=" + CustomerLoadGuid);
        $(LoadCustomerDiv + "#additionalInfo").load(domainurl + "/Leads/LeadAdditionalInfo/?CustomerId=" + CustomerLoadGuid);
        $(LoadCustomerDiv + "#CustomerContactTrackList").load(domainurl + "/Leads/LeadCustomerContactTrackList/?CustomerId=" + CustomerLoadGuid);
        $(LoadCustomerDiv + "#CreditCheck").load(domainurl + "/Leads/CreditScoreList/?CustomerId=" + CustomerLoadGuid);
        $(LoadCustomerDiv + ".CustomerACHTab").load(domainurl + "/Customer/LoadCustomerACHPaymentInfo?customerid=" + CustomerLoadGuid);
        $(LoadCustomerDiv + ".CustomerCCTab").load(domainurl + "/Customer/LoadCustomerCCPaymentInfo?customerid=" + CustomerLoadGuid);
    });
</script>

