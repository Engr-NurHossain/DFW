<div class="main-container-customer-detail">

    <div class="detail-contents">
        <div class="nav-detail-contents">
            <ul class="nav nav-tabs customer-options-tabs customer_detail_tab_style customer_detail_tab_list" id="myTab">
                <li class="active CustomerDetailTab">
                    <a tabname="#CustomerDetailTab" data-target=".CustomerDetailTab_Load" data-toggle="tab" onclick="OpenCustomerDetailTab()" @*OpenDefaultTab()*@ href="javascript:void(0)">@Localize.T("CustomerDetailTab")</a>
                </li>
                @foreach (var item in CustomerGridSetting)
                {
                    if (item.SelectedColumn == "Tab->Ticket Tab")
                    {
                        <li class="TicketTab">
                            <a tabname="#TicketTab" idval="@Model.CustomerId" data-target=".TicketTab_Load" data-toggle="tab" onclick="OpenTicketTab()">
                                @Localize.T("Tickets")
                                <span class="TicketCounter">@(Model.CustomerTabCounts.TicketsCount > 0 ? string.Format("({0})", Model.CustomerTabCounts.TicketsCount) : "")</span>
                            </a>
                        </li>
                    }
                    else if (item.SelectedColumn == "Tab->Order Tab")
                    {
                        <li class="OrderTab">
                            <a tabname="#OrderTab" class="tabOrder" data-target=".OrderTab_Load" data-toggle="tab" onclick="OpenOrderTab()" href="javascript:void(0)">
                                @(Localize.T("Orders"))
                                <span class="OrderCounter">@(Model.CustomerTabCounts.OrderCount > 0 ? string.Format("({0})", Model.CustomerTabCounts.OrderCount) : "")</span>
                            </a>
                        </li>
                    }
                    else if (item.SelectedColumn == "Tab->Contact Tab")
                    {
                        <li class="ContactTab">
                            <a tabname="#ContactTab" alttabname="#addContact" idval="@Model.CustomerId" data-target=".ContactTab_Load" data-toggle="tab" data-id="@Model.CustomerId" onclick="OpenContactTab('@Model.Soldby')">
                                @Localize.T("Contacts")
                                <span class="ContactCounter">@(Model.CustomerTabCounts.ContactCustomer > 0 ? string.Format("({0})", Model.CustomerTabCounts.ContactCustomer) : "")</span>
                            </a>
                        </li>
                    }
                    else if (item.SelectedColumn == "Tab->Estimate Tab" && PermissionChekcer.IsPermitted(Permissions.CustomerPermissions.CustomerEstimate))
                    {
                        <li class="EstimateTab">
                            <a tabname="#EstimateTab" class="tabEstimate" data-target=".EstimateTab_Load" data-toggle="tab" onclick="OpenEstimateTab()" href="javascript:void(0)">
                                @(Localize.T("Estimates"))
                                <span class="EstimateCounter">@(Model.CustomerTabCounts.EstimateCount > 0 ? string.Format("({0})", Model.CustomerTabCounts.EstimateCount) : "")</span>
                            </a>
                        </li>
                    }
                    else if (item.SelectedColumn == "Tab->Estimator Tab")
                    {
                        <li class="EstimatorTab">
                            <a tabname="#EstimatorTab" class="tabEstimator" data-target=".EstimatorTab_Load" data-toggle="tab" onclick="OpenEstimatorTab()" href="javascript:void(0)">
                                @(Localize.T("Estimators"))
                                <span class="EstimatorCounter">@(Model.CustomerTabCounts.EstimatorCount > 0 ? string.Format("({0})", Model.CustomerTabCounts.EstimatorCount) : "")</span>
                            </a>
                        </li>
                    }
                    else if (item.SelectedColumn == "Tab->Recurring Billing Tab")
                    {
                        <li class="CustomerRecurringBillingTab" title="Recurring Billing">
                            <a tabname="#CustomerRecurringBillingTab" class="tabRecurringBilling" data-target=".CustomerRecurringBillingTab_Load" data-toggle="tab" onclick="OpenCustomerRecurringBillingTab()" href="javascript:void(0)">
                                @(Localize.T("RMR"))
