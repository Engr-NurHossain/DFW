@{
    Layout = null;
    int rowcount = Model.GridGroupSetting.Count;
    float width = (100- rowcount) / rowcount;

}

@using Localize = HS.Web.UI.Helper.LanguageHelper
@using PermissionChekcer = HS.Web.UI.Helper.PermissionHelper
@using Permissions = HS.Framework.UserPermissions
@using AppConfig = HS.Framework.Utils.AppConfig;
<script type="text/x-handlebars-template" id="hbTemplate">
    <div class="filter-customer-container">
        <div class="header-content">

            <div class="custom-table">
                <div class="row less_padding table-head">
                    <div class="col-md-1 thead-th-style col_width_big custom_center customer_list_checkbox">
                        <label for="IsCheckVal1" class="label-cbx">
                            <input id="IsCheckVal1" type="checkbox" style="position:relative;" class="cus-checkBox invisible " autocomplete="off">
                            <div class="checkbox">
                                <svg width="20px" height="20px" viewBox="0 0 20 20">
                                    <path d="M3,1 L17,1 L17,1 C18.1045695,1 19,1.8954305 19,3 L19,17 L19,17 C19,18.1045695 18.1045695,19 17,19 L3,19 L3,19 C1.8954305,19 1,18.1045695 1,17 L1,3 L1,3 C1,1.8954305 1.8954305,1 3,1 Z"></path>
                                    <polyline points="4 11 8 15 16 6"></polyline>
                                </svg>
                            </div>
                        </label>
                    </div>
                    @foreach (var item in Model.GridGroupSetting)
                    {
                        <div class="clfn_table_width thead-th-style col_width_big custom_center customer-header" data-header="@item.SelectedColumn" style="width:@(width)% !important;float:left !important;">
                            <div class="customer-header-text">@Localize.T(item.SelectedColumn)</div>
                            <div class="customer-header-icon"><i class="fa fa-angle-up icon_sort_customer" data-val="ascending/@item.SelectedColumn.ToLower().Replace(" "," ")"></i><i class="fa fa-angle-down icon_sort_customer" data-val="descending/@item.SelectedColumn.ToLower().Replace(" "," ")"></i></div>
                        </div>
                    }
                </div>
            </div>
            {{#CustomerList}}
            <div class="tbody-container">
                <div class="row less_padding tr-style Export_excel_customer " data-id="">
                    <div class="table-body-td-style col-md-1 col_width_big customer_list_checkbox" valign="top">
