
@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
@using AppConfig = HS.Framework.Utils.AppConfig;
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>
@{
    string url = Request.Url.AbsolutePath;
    string DomainSitePath = AppConfig.DomainSitePath;
    if (url.ToLower().IndexOf("/myticket") > -1)
    {
        ViewBag.Title = "My Ticket";
        Html.RenderAction("LoadMyTickets", "Ticket");
    }
    if (url.ToLower().IndexOf("/ticket") > -1)
    {
        ViewBag.Title = "My Ticket";
        Html.RenderAction("TicketDashboard", "Ticket");
    }
    if (url.ToLower().IndexOf("/edit-ticket") > -1)
    {
        ViewBag.Title = "Edit Ticket";
        Html.RenderAction("TicketLiteEdit", "Ticket");
    }
    if (url.ToLower().IndexOf("/technesiantickets") > -1)
    {
        ViewBag.Title = "TechTicket";
        Html.RenderAction("TechnesianTicketPartial", "Ticket");
    }
    if (url.ToLower().IndexOf("/technesianclosetickets") > -1)
    {
        ViewBag.Title = "TechTicket";
        Html.RenderAction("TechnesianTicketPartial", "Ticket", new { status = "Closed" });
    }
    //if (url.ToLower().IndexOf("/technesianopentickets") > -1)
    //{
    //    ViewBag.Title = "TechTicket";
    //    Html.RenderAction("TechnesianTicketPartial", "Ticket", new { status = "Open" });
    //}

    else if (url.ToLower().IndexOf(DomainSitePath + "/technesianopentickets/daily") == 0)
    {
        ViewBag.Title = "Tech Upsold";
        Html.RenderAction("TechnesianTicketPartial", "Ticket", new { status = "Open", key = "Daily" });
    }
    else if (url.ToLower().IndexOf(DomainSitePath + "/technesianopentickets/weekly") == 0)
    {
        ViewBag.Title = "Tech Upsold";
        Html.RenderAction("TechnesianTicketPartial", "Ticket", new { status = "Open", key = "Weekly" });
    }
    else if (url.ToLower().IndexOf(DomainSitePath + "/technesianopentickets/monthly") == 0)
    {

        ViewBag.Title = "Tech Upsold";
        Html.RenderAction("TechnesianTicketPartial", "Ticket", new { status = "Open", key = "Monthly" });
    }
    else if (url.ToLower().IndexOf(DomainSitePath + "/technesianopentickets/yearly") == 0)
    {

        ViewBag.Title = "Tech Upsold";
        Html.RenderAction("TechnesianTicketPartial", "Ticket", new { status = "Open", key = "Yearly" });
    }
    else if (url.ToLower().IndexOf(DomainSitePath + "/technesianopentickets") == 0)
    {
        ViewBag.Title = "Tech Upsold";
        Html.RenderAction("TechnesianTicketPartial", "Ticket", new { status = "Open", key = "All" });
    }
    else if (url.ToLower().IndexOf("/ticketemailsettings") == 0)
    {
        ViewBag.Title = "Ticket Email Settings";
        Html.RenderAction("TicketEmailSetupPartial", "Ticket");
    }

}
