@{
    ViewBag.Title = "Ticket";
    Layout = null;
}
@using PermissionChekcer = HS.Web.UI.Helper.PermissionHelper
@using Permissions = HS.Framework.UserPermissions
@using AppConfig = HS.Framework.Utils.AppConfig;

@using Localize = HS.Web.UI.Helper.LanguageHelper

@Styles.Render("~/styles/ticketedit")
<script>
    var firstdate = '@ViewBag.firstdate';
    var lastdate = '@ViewBag.lastdate';
    var minHeight = window.innerHeight - 173
    //hide-widget-sidebar
    //tedit_right_block

    $(document).ready(function () {
        if (window.innerWidth < 421) {
            $(".fixed_header").width(window.innerWidth - 30);
        }
        else if (window.innerWidth < 769) {
            $(".fixed_header").width(window.innerWidth - $(".sidebar").width() - 29);
        }
        else {
            $(".fixed_header").width(window.innerWidth - $(".sidebar").width() - 52);
        }

        $(".ticket_sidebar").height(window.innerHeight - 170);
        $(".responsive__left").width(window.innerWidth - 600);
        $(".ticket_edit_rp").click(function () {
            $(".tedit_right_block").toggle();
            if ($(".tedit_right_block").css('display') === 'block') {
                $(".ticket-properties").css('right', '337px');
                $(".responsive__left").width(window.innerWidth - 850);
            }
            if ($(".tedit_right_block").css('display') === 'none') {
                $(".ticket-properties").css('right', '88px');
                $(".responsive__left").width(window.innerWidth - 600);
            }
          
        });
        $("#ticket_forward_button").click(function () {
            $(".ticket_forward_box").toggle();
        });
 });
    $(window).resize(function () {
        setTimeout(function () {
            if (window.innerWidth < 421) {
                $(".fixed_header").width(window.innerWidth - 30);
            }
            else if (window.innerWidth < 769) {
                $(".fixed_header").width(window.innerWidth - $(".sidebar").width() - 29);
            }
            else {
                $(".fixed_header").width(window.innerWidth - $(".sidebar").width() - 52);
            }
          

        }, 100);
        $(".ticket_sidebar").height(window.innerHeight - 170);
        $(".responsive__left").width(window.innerWidth - 600);
        if ($(".tedit_right_block").css('display') === 'block') {
            $(".responsive__left").width(window.innerWidth - 850);
        }
        if ($(".tedit_right_block").css('display') === 'none') {
            $(".responsive__left").width(window.innerWidth - 600);
        }
    })
</script>
<div class="content-wrap custom-head">
    <div class="container-fluid customer_container">
        <div class="row less_padding position-relative">
            <div class="clearfix fixed_header manual_top">
                <div class="customer_header_left_custom">
                    <h2 class="header_text_position">@Localize.T("Edit Ticket")</h2>
                </div>
                <div class="customer_header_right_custom">

                    <div class="AddNewBtnDiv customer_add_btn_div">

                        @if (PermissionChekcer.IsPermitted(Permissions.CustomerPermissions.ExportCustomerList))
                        {
                            <button id="CustomerReport" class="btn customer_report_btn" title="Download">
                                <i class="glyphicon glyphicon-download-alt"></i>
                            </button>
                        }


                        <div class="btn-group">
                            @if (PermissionChekcer.IsPermitted(Permissions.CustomerPermissions.CustomerCreate))
                            {
                                <button class="btn customer-new" id="AddNewCustomerList" type="button" title="@Localize.T("Add Ticket")">
                                    <i class="fa fa-plus-circle" aria-hidden="true"></i> @Localize.T(" Ticket")
                                </button>
                            }

                            @if (PermissionChekcer.IsPermitted(Permissions.CustomerPermissions.CustomerImport))
                            {
                                <button data-toggle="dropdown" class="btn dropdown-toggle customer-btn-dropdown" type="button" aria-expanded="false" title="@Localize.T("Import Tickets")">
                                    <span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu customer-dropdown-ul-style import_leads_dropdown">
                                    <li><a href="javascript:void(0)" onclick="CustomerLeadImportFile()">@Localize.T("Import Tickets")</a></li>
                                </ul>

                            }
                        </div>



                    </div>
                    <div class="dateviewed">
                        @{Html.RenderAction("DateViewPartial", "Customer");}
                    </div>
                </div>
            </div>
        </div>
        <div class="ListTicketContents">
           




        </div>
    </div>
</div>
