
@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>

@{
    string url = Request.Url.AbsolutePath;

    if (url.ToLower().IndexOf("/contact/contactdetail") > -1)
    {
        ViewBag.Title = "ContactDetails";
        Html.RenderAction("ContactDetailTab", "Contact", new { id = ViewBag.Id });
    }
    if (url.ToLower().IndexOf("/contacts") > -1)
    {
        ViewBag.Title = "CustomerDetails";
        Html.RenderAction("Contacts", "Contact"/*, new { id = ViewBag.Id }*/);
    }
}


