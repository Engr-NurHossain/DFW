                            $(".FirstEstimatorPageAgreementDocument").click(); 
                        }
                        else {
                            OpenErrorMessageNew("", data.Message, function () {

                            });
                        }
                    },
                    error: function (jqXHR, textStatus, errorThrown) {
                        console.log(errorThrown);

                    }
                });
            }
        }


