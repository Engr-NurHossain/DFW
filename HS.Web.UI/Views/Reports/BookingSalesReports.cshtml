@{
    Layout = null;
}
@using Localize = HS.Web.UI.Helper.LanguageHelper


@Styles.Render("~/styles/BookingSalesReportsCS")
@Scripts.Render("~/scripts/BookingSalesReportsJS")

<div class="ReportsTitle">
    @Localize.T("Reports")
    <div class="dateviewed">
        @{Html.RenderAction("DateViewPartial", "Customer");}
    </div>
</div>


<div class="col-xs-12 Report-Tabs">
    <div class="nav-detail-contents">
        <ul class="nav nav-tabs payroll_report_top_nav" id="SalesReportForRug">
            <li class="SalesReportTab active">
                <a idval="" data-target="#SalesReportTab" data-toggle="tab">@Localize.T("Booking")</a>
            </li>
            <li class="FinishedJobTab">
                <a idval="" data-target="#FinishedJobTab" data-toggle="tab">@Localize.T("Finished Job")</a>
            </li>
            @*<li class="PackageSummaryTab">
                <a idval="" data-target="#PackageSummaryTab" data-toggle="tab">@Localize.T("Package Summary")</a>
            </li>*@ 
        </ul>
        <div class="tab-content reports_tab_content">
            <div class="tab-pane active" id="SalesReportTab">

                <div class="Load_Sales_Report">

                </div>
            </div>
            <div class="tab-pane" id="FinishedJobTab">

                <div class="FinishedJobTab_Report">

                </div>
            </div>
            @*<div class="tab-pane" id="PackageSummaryTab">

                <div class="PackageSummaryTab_Report">

                </div>
            </div>*@

        </div>
    </div>
</div>


