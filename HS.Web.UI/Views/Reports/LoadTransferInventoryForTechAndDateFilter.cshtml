@{
    Layout = null;
}
@using Localize = HS.Web.UI.Helper.LanguageHelper

@model List<HS.Entities.InventoryTech>
@using PermissionChekcer = HS.Web.UI.Helper.PermissionHelper
@using Permissions = HS.Framework.UserPermissions

<link href="~/Content/Css/Report/LeadsReportPartial.css" rel="stylesheet" />
<link href="~/Content/Css/Ticket/TicketListPartial.css" rel="stylesheet" />
<script>
    $(document).ready(function () {
        $(".inven_summery_nounits_container_inner").height(window.innerHeight - 70);
    });
    $(window).resize(function () {
        $(".inven_summery_nounits_container_inner").height(window.innerHeight - 70);
    })
</script>

<div class="inven_summery_nounits_container">
   
    <div class="inven_summery_nounits_container_inner">
        <table id="LeadsReportTable" class="inventory_report_table">
            <thead>
                <tr>
                    <th>@Localize.T("Category")</th>
                    <th>@Localize.T("Manufacturer")  </th>
                    <th>@Localize.T("Description")</th>
                    <th>@Localize.T("Transfer From")</th>
                    <th>@Localize.T("SKU")</th>
                    <th>@Localize.T("User Name")</th>
                    <th>@Localize.T("Date Transferred")</th>
                    <th>@Localize.T("Quantity")</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
            {
                    <tr>
                        <td>
                            <div>@Localize.T("Category") <span>:</span></div>
                            <div>@item.Category</div>
                        </td>
                        <td>
                            <div>@Localize.T("Manufacturer") <span>:</span></div>
                            <div> @item.Manufacture</div>
                        </td>
                        <td>
                            <div>@Localize.T("Description") <span>:</span></div>
                            <div>@item.EquipName</div>
                        </td>
                        <td>
                            <div>@Localize.T("Transfer From") <span>:</span></div>
                            <div>@item.Desc</div>
                        </td>
                        <td>
                            <div>@Localize.T("SKU") <span>:</span></div>
                            <div>@item.Sku</div>
                        </td>
                        <td>
                            <div>@Localize.T("User Name") <span>:</span></div>
                            <div>@item.TechnicianName</div>
                        </td>
                        <td>
                            <div>@Localize.T("Date Transferred") <span>:</span></div>
                            <div>
                                @item.LastUpdatedDate.ToString("MM/dd/yyyy")
                            </div>
                        </td>
                        <td>
                            <div>@Localize.T("Quantity") <span>:</span></div>
                            <div>
                                @item.Quantity
                            </div>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    
</div>



