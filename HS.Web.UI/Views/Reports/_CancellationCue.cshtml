@{
    Layout = null;
}
@using Localize = HS.Web.UI.Helper.LanguageHelper

@Styles.Render("~/styles/CancellationCue")
@Scripts.Render("~/scripts/CancellationCue")

<div class="cencellation_queue_header_div">
    <div class="cencellation_queue_header"> @Localize.T("Reports")</div>
    <div class="dateviewed">
        @{Html.RenderAction("DateViewPartial", "Customer");}
    </div>
</div>


<div class="col-xs-12 Report-Tabs">
    <div class="nav-detail-contents">
        <ul class="nav nav-tabs reports-tabs" id="myTab">
            <li class="CustomerCancellationNav active">
                <a data-target="#CustomerCancellationTab" data-toggle="tab">@Localize.T("Cancellation Queue")</a>
            </li>

            <li class="CancelledCustomerNav">
                <a data-target="#CancelledCustomerTab" data-toggle="tab">@Localize.T("Cancelled Customer")</a>
            </li>
        </ul>

        <div class="tab-content reports_tab_content">
            <div class="tab-pane active" id="CustomerCancellationTab">
                <div class="CustomerCancellation_report">

                </div>
            </div>
            <div class="tab-pane" id="CancelledCustomerTab">
                <div class="CancelledCustomer_report">

                </div>
            </div>
        </div>
           
        </div>
    </div>


