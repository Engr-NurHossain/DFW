@{
    Layout = null;
}
@using Localize = HS.Web.UI.Helper.LanguageHelper
@using AppConfig = HS.Framework.Utils.AppConfig;

<link href="~/Content/Css/PackageSettings/LoadPacakageOptionalSettingsPartial.css" rel="stylesheet" />
<script>
    $(document).ready(function () {
        parent.$('.close').click(function () {
            parent.$(".modal-body").html('');
        })
        $(".LoaderWorkingDiv").hide();
        $("#AddMapTypeService").click(function () {
            OpenRightToLeftModal("@(AppConfig.DomainSitePath)/SmartPackageSetup/AddMapTypeService/?Id=0");
        });
        $(".LoadMapTypeService").load("@(AppConfig.DomainSitePath)/SmartPackageSetup/LoadMapTypeServicePartial");
    })
</script>
<style>

</style>
<div>

    <div class="custom-padding-20px">

    </div>

    <div>
        <button class="btn add-package-optional" id="AddMapTypeService" type="button">
            <i class="fa fa-plus-circle" aria-hidden="true"></i> @Localize.T(" New")
        </button>
    </div>

    <div class="LoadMapTypeService">

    </div>


</div>

