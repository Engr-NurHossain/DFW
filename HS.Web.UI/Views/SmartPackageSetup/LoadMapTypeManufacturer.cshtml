@{
    Layout = null;
}
@using Localize = HS.Web.UI.Helper.LanguageHelper
@using AppConfig = HS.Framework.Utils.AppConfig;

<link href="~/Content/Css/PackageSettings/LoadPacakageOptionalSettingsPartial.css" rel="stylesheet" />
@*<script src="~/Content/Js/PackageSetup/LoadPackageSmartSystemTypeSettingsPartial.js"></script>*@
<script>
    $(document).ready(function () {
        parent.$('.close').click(function () {
            parent.$(".modal-body").html('');
        })
        $(".LoaderWorkingDiv").hide();
        $("#add-package-map-smart-install-system-manu").click(function () {
            OpenRightToLeftModal("@(AppConfig.DomainSitePath)/SmartPackageSetup/AddMapTypeManufacturer/?Id=0");
        });
        $(".company-package-smart-system-install-type-list-div-manu").load("@(AppConfig.DomainSitePath)/SmartPackageSetup/LoadMapTypeManufacturerPartial");
    })
</script>
<style>

</style>
<div>

    <div class="custom-padding-20px">

    </div>

    <div>
        <button class="btn add-package-optional" id="add-package-map-smart-install-system-manu" type="button">
            <i class="fa fa-plus-circle" aria-hidden="true"></i> @Localize.T(" New")
        </button>
    </div>

    <div class="company-package-smart-system-install-type-list-div-manu">

    </div>


</div>
