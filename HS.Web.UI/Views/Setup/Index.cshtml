@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>
@{
    string url = Request.Url.AbsolutePath;

    if (url.ToLower().IndexOf("/setupdetailsinfo") == 0)
    {
        ViewBag.Title = "Settings";
        Html.RenderAction("SetupDetails", "Setup");
    }

    else if (url.ToLower().IndexOf("/uisetupdetails") == 0)
    {
        ViewBag.Title = "Settings";
        Html.RenderAction("SmartUiSetup", "Setup");
    }
    else if (url.ToLower().IndexOf("/gridsettings") == 0)
    {
        ViewBag.Title = "Grid Settings";
        Html.RenderAction("GridSettings", "Setup");
    }
    else if (url.ToLower().IndexOf("/mmrsinfo") == 0)
    {
        ViewBag.Title = "MMR";
        Html.RenderAction("MMRS", "Setup");
    }
    else if (url.ToLower().IndexOf("/setupdropdowns") == 0)
    {
        ViewBag.Title = "Dropdown List Setup";
        Html.RenderAction("LookupSetup", "Setup");
    }
    else if (url.ToLower().IndexOf("/localization") == 0)
    {
        ViewBag.Title = "LocalizeResource Setup";
        Html.RenderAction("LocalizeResource", "Setup");
    }
    //else if (url.ToLower().IndexOf("/buildVersiondropdowns") == 0)
    //{
    //    ViewBag.Title = "Build Version";
    //    Html.RenderAction("BuildVersion", "Setup");
    //}
}