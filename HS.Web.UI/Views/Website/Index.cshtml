
@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>

@using AppConfig = HS.Framework.Utils.AppConfig;

@{
    string url = Request.Url.AbsolutePath.ToLower();
    string DomainSitePath = AppConfig.DomainSitePath;
    if (url.IndexOf(DomainSitePath + "/website/websitedetail") > -1)
    {
        ViewBag.Title = "WebsiteDetails";
        Html.RenderAction("DetailTab", "Website", new { id = ViewBag.Id });
    }
    //else if (url.IndexOf(DomainSitePath + "/menumanagement") == 0)
    //{
    //    ViewBag.Title = "MenuManagement";
    //    Html.RenderAction("MenuManagementPartial", "MenuManagement");
    //}
    else if (url.IndexOf(DomainSitePath + "/title-settings") == 0)
    {
        ViewBag.Title = "Website";
        Html.RenderAction("Website", "Website");
    }
}
