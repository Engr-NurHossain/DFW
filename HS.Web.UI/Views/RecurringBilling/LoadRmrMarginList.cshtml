<style>
    .lra_height {
        width:100%;
        float:left;
        overflow:auto;
    }
    .lra_height table tr th:first-child {
        left: -1px;
        z-index: 10;
    }

    .lra_height table tr td:first-child {
        position: sticky;
        left: -1px;
        background-color: #fff;
    }

    .lra_height table tr:nth-child(odd) td:first-child {
        background-color: #f4f5f8;
    }
    .lra_height table tr th {
        white-space: nowrap;
    }
        .lra_height table tr th div:nth-child(1) {
            padding-right: 15px;
        }
    .lra_height table {
        table-layout:unset !important;
    }
</style>
<script>
    var Currency = "@HS.Web.UI.Helper.LabelHelper.CurrentTransMakeCurrency.MakeCurrency(null)";
