@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>

@using AppConfig = HS.Framework.Utils.AppConfig;
@{

    string DomainSitePath = AppConfig.DomainSitePath;
    string url = Request.Url.AbsolutePath.ToLower();
    if (url.IndexOf(DomainSitePath + "/credentialsettinginfo") == 0)
    {
        ViewBag.Title = "Credential Setting Info";
        Html.RenderAction("CredentialSettingPartial", "CredentialSetting");
    }
}
