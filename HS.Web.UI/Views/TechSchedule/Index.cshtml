@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>
@{
    string url = Request.Url.AbsolutePath;

    if (url.ToLower().IndexOf("/TechSchedule") == 0)
    {
        ViewBag.Title = "TechSchedule";
        Html.RenderAction("TechSchedulePartial", "TechSchedule");
    }
    else if (url.ToLower().IndexOf("/techSchedulesettinginfo") == 0)
    {
        ViewBag.Title = "TechSchedule Setting Info";
        Html.RenderAction("TechScheduleSettingPartial", "TechSchedule");
    }
}
