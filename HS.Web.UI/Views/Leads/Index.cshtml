
@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
@using AppConfig = HS.Framework.Utils.AppConfig; 
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script> 
@{
    string url = Request.Url.AbsolutePath.ToLower() ;
    string DomainSitePath = AppConfig.DomainSitePath;
    if (url.IndexOf(DomainSitePath +"/lead/leadsdetail") == 0)
    {
        ViewBag.Title = "Leads Details";
        Html.RenderAction("LeadsDetails", "Leads", new { id = ViewBag.id });
    }

    else if (url.IndexOf(DomainSitePath +"/leadverifyinfo/") == 0)
    {
        ViewBag.Title = "Lead";
        Html.RenderAction("VerifyLeadPartial", "Leads", new { id = ViewBag.id });
    }
    else if (url.IndexOf(DomainSitePath + "/leadimportfromcms") == 0)
    {
        ViewBag.Title = "Lead Import From CMS";
        Html.RenderAction("LeadImportFromCMS", "Leads");
    }
    else if (url.IndexOf(DomainSitePath + "/lead-list") == 0)
    {
        ViewBag.Title = "Lead";
        Html.RenderAction("LeadsLitePartial", "Leads");
    }
    else if (url.IndexOf(DomainSitePath +"/lead") == 0)
    {
        ViewBag.Title = "Lead";
        Html.RenderAction("LeadsLitePartial", "Leads");
    }

    else if (url.IndexOf(DomainSitePath +"/createlead") == 0)
    {
        ViewBag.Title = "Create Lead";
        Html.RenderAction("LeadFormGenerationPartial", "Leads", new { LeadId = ViewBag.LeadId });
    }
    else if (url.IndexOf(DomainSitePath +"/packagesettings") == 0)
    {
        ViewBag.Title = "Package Settings";
        Html.RenderAction("PackageSettingsPartial", "Leads");
    }

    //if (url.IndexOf(DomainSitePath +"/leadverifyinfo") == 0)
    //{
    //    ViewBag.Title = "Lead Verify Info";
    //    Html.RenderAction("VerifyLeadPartial", "Leads");
    //}
    //if (url.IndexOf(DomainSitePath +"/leadsetupinfo") == 0)
    //{
    //    ViewBag.Title = "Lead Setup Info";
    //    Html.RenderAction("LeadSetupPartial", "Leads");
    //}
}
