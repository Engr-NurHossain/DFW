@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>
@{
    string url = Request.Url.AbsolutePath;
    if (url.ToLower().IndexOf("/leadsverificationdetail") > -1)
    {
        ViewBag.Title = "Lead Edit Info";
        Html.RenderAction("LeadVerificationPartial", "Leads");
    }
      if (url.ToLower().IndexOf("/leadsinfo") > -1)
    {
        ViewBag.Title = "Lead Edit Info";
        Html.RenderAction("LeadInfoPartial", "Leads");
    }
    //if (url.ToLower().IndexOf("/leads/leadSetupPartial") > -1)
    //{
    //    ViewBag.Title = "Lead Setup Info";
    //    Html.RenderAction("LeadSetupPartial", "Leads");
    //}
    if (url.ToLower().IndexOf("/leadverifyinfo") > -1)
    {
        ViewBag.Title = "Lead Verify Info";
        Html.RenderAction("VerifyLeadPartial", "Leads");
    }
    if (url.ToLower().IndexOf("/documentcenterinfo") == 0)
    {
        ViewBag.Title = "Files/Documents";
        Html.RenderAction("DocumentCenterPartial", "Leads");
    }
    if (url.ToLower().IndexOf("/leadverifyandsetupdetail") == 0)
    {
        ViewBag.Title = "Lead Verify & Setup Detail";
        Html.RenderAction("LeadVerifyAndSetupDetailInformationPartial", "Leads");
    }
}


