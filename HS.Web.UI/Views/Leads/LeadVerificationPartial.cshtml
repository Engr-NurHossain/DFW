                <div id="CreditScore" class="lead_acc_info_container lead_acc_info_margin_top">
                    <div class="lead_acc_info_header">@Localize.T("Credit Score")</div>
                    <table>
                        <thead>
                            <tr>
                                <th>Name</th>
                                @if (PermissionChecker.IsPermitted(PermissionList.LeadPermissions.LeadCreditScore))
                                {
                                    <th>Score</th>
                                }
                                @if (PermissionChecker.IsPermitted(PermissionList.LeadPermissions.LeadCreditScoreGrade))
                                {
                                    <th>Grade</th>
                                }
                                <th>Status</th>
                                @if (PermissionChecker.IsPermitted(PermissionList.LeadPermissions.LeadCreditCheckReport))
                                {
                                    <th>Download</th>
                                }
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>@Model.FirstName @Model.LastName</td>

                                @if (Model.CreditCheck != null)
                                {
                                    double CreditScore = 0;
                                    double.TryParse(Model.CreditCheck.Score, out CreditScore);
                                    if (PermissionChecker.IsPermitted(PermissionList.LeadPermissions.LeadCreditScore))
                                    {
                                        int score = 0;
                                        int.TryParse(Model.CreditCheck.Score, out score);
                                        <td class="score">@score</td>
                                    }
                                    if (PermissionChecker.IsPermitted(PermissionList.LeadPermissions.LeadCreditScoreGrade))
                                    {
                                        <td class="grade">@Model.CreditCheck.Grade</td>
                                    }
                                    <td class="hitStatus">@Model.CreditCheck.Hit</td>
                                    if (PermissionChecker.IsPermitted(PermissionList.LeadPermissions.LeadCreditCheckReport))
                                    {
                                        <td><a id="CreditRecordpdf" href="javascript:void(0);">@Model.CreditCheck.RepontPdfName</a></td>
                                    }

                                }
                                else
                                {
                                    if (PermissionChecker.IsPermitted(PermissionList.LeadPermissions.LeadCreditScore))
                                    {
                                        <td class="score"></td>
                                    }
                                    if (PermissionChecker.IsPermitted(PermissionList.LeadPermissions.LeadCreditScoreGrade))
                                    {
                                        <td class="grade"></td>
                                    }
                                    <td class="hitStatus"></td>
                                    if (PermissionChecker.IsPermitted(PermissionList.LeadPermissions.LeadCreditCheckReport))
                                    {
                                        <td><a id="CreditRecordpdf" href="javascript:void(0);"></a></td>
                                    }
                                }
                            </tr>
                        </tbody>
                    </table>
