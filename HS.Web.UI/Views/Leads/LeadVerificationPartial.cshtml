    var CustomerName = '@(Model.Type == "Commercial" && Model.BusinessName != ""? Model.BusinessName:string.Format("{0} {1}", Model.FirstName, Model.LastName))';
    var LeadGuid = CustomerGuidId;
    var creditGrade = '@ViewBag.CreditGrade';
    var CreditScoreGradeId = '@Model.CreditScore';
       var BrinksCreditCheck = '@ViewBag.BrinksCreditCheck';
    var HasDiffrentCreditContact = '@ViewBag.HasDiffrentCreditContact';
    accountType = JSON.parse(accountType);

    // Function to validate first name for special characters
    function validateFirstName() {
        var firstName = $("#FirstName").val();
        if (!(/^[a-zA-Z"'() ]*$/u.test(firstName))) {
            parent.OpenErrorMessageNew("Error!", "First name must contain only letters", function () { });
            $("#FirstName").val('');
            return false;
        }
        return true;
    }
    // Function to validate last name for special characters
    function validateLastName() {
        var lastName = $("#LastName").val();
        if (!(/^[a-zA-Z"'() ]*$/u.test(lastName))) {
            parent.OpenErrorMessageNew("Error!", "Last name must contain only letters", function () { });
            $("#LastName").val('');
            return false;
        }
        return true;
    }

    $(document).ready(function () {
       @if (ViewBag.OwnerShipList != null)
       {

           foreach (var item in ViewBag.OwnerShipList)
           {
              if (item.Value == "In-House")
              {
              <text>
               if ($("#Ownership").val() == "-1")
               {

                $("#Ownership").val("In-House");

               }

              </text>
              }
           }
       }
        $("#FirstName").focusout(function () {
            validateFirstName();
        });

        $("#LastName").focusout(function () {
            validateLastName();
        });
     });
</script>
