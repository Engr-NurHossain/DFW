@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>
@{
    string url = Request.Url.AbsolutePath;

    if (url.ToLower().IndexOf("/smartleadsetup") > -1)
    {
        ViewBag.Title = "Smart Lead Setup Info";
        Html.RenderAction("SmartLeadSetupPartial", "SmartLeads");
    }
    if (url.ToLower().IndexOf("/aggrementqstn") > -1)
    {
        ViewBag.Title = "Aggrement Question";
        Html.RenderAction("AggrementQstn", "SmartLeads");
    }
}


