    $(document).ready(function () {
        $(".LoaderWorkingDivBrinks").hide();
        $(".add_brinks_info_container").height(window.innerHeight - 92);

        $("#IsSigned").change(function () {
            SaveBrinksSignedInfo();
        })

        $("#HasBillingInfo").change(function () {
            SaveBrinksSignedInfo();
        })
         $("#IsCreditCheck").change(function () {
            SaveBrinksSignedInfo();
        })
        
        $("#HasBusinessPicture").change(function () {
            SaveBrinksSignedInfo();
        })
        $("#Phone1").keyup(function () {

            var PhoneNumber = $(this).val();
             console.log(PhoneNumber);
            if (PhoneNumber != 'undefined' && PhoneNumber != null && PhoneNumber != "") {
                var cleanPhoneNumber = FormatePhoneNumber(PhoneNumber);
                $(this).val(cleanPhoneNumber);
            }
        });

            var Value = $("#Receiver_Phone").val();
            Value = FormateReceiverNumber(Value);;
            $("#Receiver_Phone").val(Value);
          $("#Receiver_Phone").keyup(function () {

            var PhoneNumber = $(this).val();
             console.log(PhoneNumber);
            if (PhoneNumber != 'undefined' && PhoneNumber != null && PhoneNumber != "") {
                var cleanPhoneNumber = FormateReceiverNumber(PhoneNumber);
                $(this).val(cleanPhoneNumber);
            }
          });
          $("#panel_phone").keyup(function () {

            var PhoneNumber = $(this).val();
             console.log(PhoneNumber);
            if (PhoneNumber != 'undefined' && PhoneNumber != null && PhoneNumber != "") {
                var cleanPhoneNumber = FormatePanelNumber(PhoneNumber);
                $(this).val(cleanPhoneNumber);
            }
          });
        $("#phone1").keyup(function () {

            var PhoneNumber = $(this).val();
             console.log(PhoneNumber);
            if (PhoneNumber != 'undefined' && PhoneNumber != null && PhoneNumber != "") {
                var cleanPhoneNumber = Formatephone1(PhoneNumber);
                $(this).val(cleanPhoneNumber);
            }
          });
             $(".agencyPhone1").keyup(function () {

            var PhoneNumber = $(this).val();
             console.log(PhoneNumber);
            if (PhoneNumber != 'undefined' && PhoneNumber != null && PhoneNumber != "") {
                var cleanPhoneNumber = FormateAgencyNumber(PhoneNumber);
                $(this).val(cleanPhoneNumber);
            }
        });

        StartDatepicker = new Pikaday({

            format: 'MM/DD/YYYY',
            field: $('#install_date')[0],
     
            firstDay: 1,
       

      
        });
        $("#Zonelist").load("/API/CustomerZoneList?CustomerId=" + CustomerLoadGuid + "&&Platform='Brinks'");
        $("#Agencylist").load("/API/CustomerAgencylist?CustomerId=" + CustomerLoadGuid + "&&Platform='Brinks'");
        $("#Contactlist").load("/API/CustomerEmgContactlist?CustomerId=" + CustomerLoadGuid + "&&Platform='Brinks'");
        $("#SubscribeToBrinks").click(function () {
            var result = false;
            if ($("#IsSigned").is(':checked') == true && $("#HasBillingInfo").is(':checked') == true && $("#HasBusinessPicture").is(':checked') == true && $("#IsCreditCheck").is(':checked') == true) {
                result = true;
            }
            else {
                result = false;
            }
            if (BrinksMendatoryField == "true") {
                if (result == true) {
                    SubscribeToBrinksConfirm();
                }
                else {
                    OpenErrorMessageNew("Error!", "You have to check all the four check items.", function () {
                    });
                }
            }
            else {
                 SubscribeToBrinksConfirm();
            }
          
      
        });

        $("#Zode_Addition").click(function () {

            if ($("#BrinksZoneNumber1").val() == "-1") {
                $("#errorZoneNumber").removeClass("hidden");
            }
            else {
                $("#errorZoneNumber").addClass("hidden");
            }
            if ($("#BrinksEventCode1").val() == "-1") {
                $("#errorEventCode").removeClass("hidden");
            }
            else {
                $("#errorEventCode").addClass("hidden");
            }

            var param = {
                "ID":    $("#ZoneId").val(),
                "CustomerId": CustomerLoadGuid,
                "ZoneNumber": $("#BrinksZoneNumber1").val(),
                "EventCode": $("#BrinksEventCode1").val(),
                "Location": $("#BrinksEquipLocation1").val(),
                "EquipmentType": $("#BrinksEquipType").val(),
                "ZoneComment": $("#ZoneComment").val(),
                "Platform": "Brinks"

            };
            if (BrinksZoneValidation() == true) {
                console.log(param);
                $.ajax({
                    type: "POST",
                    ajaxStart: $(".loader-div").show(),
                    url: "/Customer/AddUccZone",
                    data: JSON.stringify(param),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    cache: false,
                    success: function (data) {
                        $(".LoaderWorkingDiv").hide();
                        if (typeof (data) != 'undefined') {

                            $("#ZoneId").val("");
                            $("#BrinksZoneNumber1").val("");
                            $("#BrinksEventCode1").val("-1");
                            $("#BrinksEquipType").val("-1");
                            $("#BrinksEquipLocation1").val("-1");
                            $("#ZoneComment").val("");
                            $("#Zonelist").load("/API/CustomerZoneList?CustomerId=" + CustomerLoadGuid + "&&Platform='Brinks'");

                        }
                    },
                    error: function (jqXHR, textStatus, errorThrown) {
                        $(".loader-div").hide();
                        console.log(errorThrown);
                    }
                });

            }
            else {

            }

        })
        $("#Get_Agencies").click(function () {

            var param = {
                "ZipCode": $("#ZipCode").val(),
                 "CustomerId": CustomerLoadGuid,
            };

            console.log(param);
            $.ajax({
                type: "POST",
                ajaxStart: $(".loader-div").show(),
                url: "/API/GetThirdpartyAgencies",
                data: JSON.stringify(param),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                cache: false,
                success: function (data) {
                    $(".LoaderWorkingDiv").hide();
                    if (typeof (data) != 'undefined') {
                        if (data.result == true) {
                            $("#Agencylist").load("/API/CustomerAgencylist?CustomerId=" + CustomerLoadGuid + "&&Platform='Brinks'");
                        }
                        else {
                            OpenErrorMessageNew("Error!", data.message, function () {

                            });
                        }


                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $(".loader-div").hide();
                    console.log(errorThrown);
                }
            });


        })

        $("#Agency_Addition").click(function () {

            var param = {
                "CustomerId": CustomerLoadGuid,
                "Agencytype": $("#agencyType").val(),
                "Phone": $(".agencyPhone1").val(),
                "PermType": $("#PremType").val(),
                "Platform": "Brinks"
            };

            console.log(param);
            $.ajax({
                type: "POST",
                ajaxStart: $(".loader-div").show(),
                url: "/API/AddBrinksAgency",
                data: JSON.stringify(param),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                cache: false,
                success: function (data) {
                    $(".LoaderWorkingDiv").hide();
                    if (typeof (data) != 'undefined') {
                        $("#Agencylist").load("/API/CustomerAgencylist?CustomerId=" + CustomerLoadGuid + "&&Platform='Brinks'");

                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $(".loader-div").hide();
                    console.log(errorThrown);
                }
            });


        })
        $("#Contact_Addition").click(function () {
            var Haskey = "False";
            if ($('#Haskey').is(':checked') == true) {
                Haskey = "True";
            }
            var param = {
                "Id":    $("#ContactId").val(),
                "CustomerId": CustomerLoadGuid,
                "FirstName":    $("#FirstName1").val(),
                "LastName": $("#LastName1").val(),
                "Phone": $("#Phone1").val(),
                "RelationShip": $("#RelationShip").val(),
                "HasKey":Haskey,

                "Platform": "Brinks"

            };

            console.log(param);
            $.ajax({
                type: "POST",
                ajaxStart: $(".loader-div").show(),
                url: "/Customer/AddEmergencyContact",
                data: JSON.stringify(param),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                cache: false,
                success: function (data) {
                    $(".LoaderWorkingDiv").hide();
                    if (typeof (data) != 'undefined') {
                        $("#ContactId").val("");
                        $("#FirstName1").val(""),
                        $("#LastName1").val(""),
                        $("#Phone1").val("")
                        $("#Contactlist").load("/API/CustomerEmgContactlist?CustomerId=" + CustomerLoadGuid + "&&Platform='Brinks'");

                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $(".loader-div").hide();
                    console.log(errorThrown);
                }
            });


        })
        $("#BrinksEventCode1").change(function () {

            GetEquipmentType();
        });

    })
</script>
