    var SubscribeToBrinks = function () {
        var url = "/API/AddBrinksAccount/";
        BrinksEmergencyContact = {
            FirstName1: $("#FirstName1").val(),
            LastName1: $("#LastName1").val(),
            Phone1: $("#Phone1").val(),
            PhoneType1: $("#PhoneType1").val(),

            FirstName2: $("#FirstName2").val(),
            LastName2: $("#LastName2").val(),
            Phone2: $("#Phone2").val(),
            PhoneType2: $("#PhoneType2").val(),

            FirstName3: $("#FirstName3").val(),
            LastName3: $("#LastName3").val(),
            Phone3: $("#Phone3").val(),
            PhoneType3: $("#PhoneType3").val(),

            FirstName4: $("#FirstName4").val(),
            LastName4: $("#LastName4").val(),
            Phone4: $("#Phone4").val(),
            PhoneType4: $("#PhoneType4").val(),
        };
        var param = {
            "cs_no": $("#cs_no").val(),
            "site_name": $("#site_name").val(),
            "codeword1": $("#codeword1").val(),
            "site_addr1": $("#site_addr1").val(),
            "sitestat_id": $("#sitestat_id").val(),
            "sitetype_id": $("#sitetype_id").val(),
            "subdivision": $("#subdivision").val(),
            "systype_id": $("#systype_id").val(),
            "sec_systype_id": $("#sec_systype_id").val(),
            "Receiver_Phone": $("#Receiver_Phone").val(),
            "panel_phone": $("#panel_phone").val(),
            "phone1": $("#phone1").val(),
            "panel_location": $("#panel_location").val(),
            "install_date": $("#install_date").val(),
            "panel_code": $("#panel_code").val(),
            "city_name": $("#City").val(),
            "state_id": $("#State").val(),
            "county_name": $("#County").val(),
            "zip_code": $("#ZipCode").val(),

            "CustomerId": CustomerLoadGuid,
            "install_servco_no": $("#install_servco_no").val(),
            "servco_no": $("#servco_no").val(),

            BrinksemgContact: BrinksEmergencyContact
        };
        console.log(param);
        $(".LoaderWorkingDivBrinks").show();
        $.ajax({
            type: "POST",
            ajaxStart: $(".loader-div").show(),
            url: url,
            data: JSON.stringify(param),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            cache: false,
            success: function (data) {
                $(".LoaderWorkingDivBrinks").hide();
                if (typeof (data) != 'undefined') {
                    if (data) {
                        if (data.result) {
                            OpenSuccessMessageNew("Success!", data.message, function () {
                                CloseTopToBottomModal();
                                OpenBrinksDetails();
                            });
                        } else {
                            OpenErrorMessageNew("Error!", data.message, function () {

                                OpenBrinksDetails();
                            });
                        }
                    } else {
                        OpenErrorMessageNew("Error!", data.message, function () {

                        });
                    }
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {
                $(".loader-div").hide();
                console.log(errorThrown);
            }
        });
    }

    var GetEquipmentType = function () {
            var param = {
                "EventCode": $("#BrinksEventCode1").val(),
            };
            console.log(param);
            $.ajax({
                type: "POST",
                ajaxStart: $(".loader-div").show(),
                url: "/API/GetEquipmentType",
                data: JSON.stringify(param),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                cache: false,
                success: function (data) {
                    $(".LoaderWorkingDiv").hide();
                    if (typeof (data.data) != 'undefined') {
                        $("#BrinksEquipType option").remove();
                        var resultparse = JSON.parse(data.data);
                        $.each(resultparse, function (i, word) {

                            $("#BrinksEquipType").append("<option value=" + word.EquipmentTypeId + ">" + word.EquipTypeVal + "</option>");
                        });
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $(".loader-div").hide();
                    console.log(errorThrown);
                }
            });
    }
    var SaveBrinksSignedInfo = function () {
             
            var param = {
                "CustomerId":CustomerLoadGuid,
                "IsSigned": $("#IsSigned").is(':checked'),
                "HasBillingInfo": $("#HasBillingInfo").is(':checked'),
                "HasBusinessPicture": $("#HasBusinessPicture").is(':checked'),
                "IsCreditCheck":$("#IsCreditCheck").is(':checked')
            };
            console.log(param);
            $.ajax({
                type: "POST",
           
                url: "/API/SaveBrinksSignedInfo",
                data: JSON.stringify(param),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                cache: false,
                success: function (data) {
                 
                },
                error: function (jqXHR, textStatus, errorThrown) {
                   
                    console.log(errorThrown);
                }
            });
    }

    var BrinksZoneValidation = function () {
        if ($("#BrinksZoneNumber1").val() == "-1" || $("#BrinksEventCode1").val() == "-1") {
            return false;
        }
        return true;
    }

    function FormatePhoneNumber(Value) {
        var ValueClean = "";
        if (Value != undefined && Value != "" && Value != null) {
            Value = Value.replace(/[-()  ]/g, '');
            if (Value.length == 10) {
                ValueClean = Value.replace(/(\d{3})\-?(\d{3})\-?(\d{4})/, "$1-$2-$3");
                $("#Phone1").css({ "border": "1px solid #babec5" });
            }
            else if (Value.length > 10) {
                ValueClean = Value;
                $("#Phone1").css({ "border": "1px solid red" });
            }
            else {
                $("#Phone1").css({ "border": "1px solid red" });
                ValueClean = Value;
            }
        }
        return ValueClean;
    }
