@using Localize = HS.Web.UI.Helper.LanguageHelper

<style>
    .techDetails {
        color: #2ca01c;
        font-weight: 600;
        cursor: pointer;
    }

    .technicianList {
        width: 317px;
        float: left;
    }

    .width-100per {
        width: 100%;
        padding-top: 5px;
    }

    .technicianListHeader {
        background-color: #ccc;
        padding: 11px 5px;
    }

