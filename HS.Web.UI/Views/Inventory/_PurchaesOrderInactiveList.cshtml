        font-size: 15px;
        font-weight: 600;
        text-align: center;
    }

    .technicianListContent {
        border-bottom: 1px solid #aaa;
        padding-top: 5px;
        padding-bottom: 5px;
        padding-left: 5px;
        padding-right: 5px;
    }

    .technicianListContent:hover {
        cursor: pointer;
        background-color: #ccc;
        opacity: 0.9;
    }

    .individualTechnicianDetail {
        float: right;
    }

    .NoInventoryDiv {
