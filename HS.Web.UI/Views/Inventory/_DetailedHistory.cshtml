                                </div>
                                <div class="NavigatonButtonsDiv">
                                    <button class="NavigationButton activeBtn" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers), @(ViewBag.order))">@(ViewBag.PageNumberTransfers)</button>
                                </div>
                                <div class="paginationRightButtondv">
                                    <button class="PaginationButtons disabledBtn" onclick="OpenDetailsByEquipmentStart()">Next</button>
                                </div>
                            }
                            else if (ViewBag.PageNumberTransfers == 1)
                            {
                                <div class="paginationMiddleButtondv">
                                    <button class="PaginationButtons" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers-1), @(ViewBag.order))">Previous</button>
                                </div>
                                <div class="NavigatonButtonsDiv">
                                    <button class="NavigationButton activeBtn">@(ViewBag.PageNumberTransfers)</button>
                                </div>
                                <div class="NavigatonButtonsDiv">
                                    <button class="NavigationButton" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers+1), @(ViewBag.order))">@(ViewBag.PageNumberTransfers + 1)</button>
                                </div>
                                <div class="NavigatonButtonsDiv">
                                    <button class="NavigationButton" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers + 2), @(ViewBag.order))">@(ViewBag.PageNumberTransfers + 2)</button>
                                </div>
                                <div class="paginationRightButtondv">
                                    <button class="PaginationButtons" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers+1), @(ViewBag.order))">Next</button>
                                </div>
                            }
                            else if (ViewBag.PageNumberTransfers == 2)
                            {
                                <div class="paginationMiddleButtondv">
                                    <button class="PaginationButtons" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers-1), @(ViewBag.order))">Previous</button>
                                </div>
                                <div class="NavigatonButtonsDiv">
                                    <button class="NavigationButton" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers-1), @(ViewBag.order))">@(ViewBag.PageNumberTransfers - 1)</button>
                                </div>
                                <div class="NavigatonButtonsDiv">
                                    <button class="NavigationButton activeBtn">@(ViewBag.PageNumberTransfers)</button>
                                </div>
                                <div class="NavigatonButtonsDiv">
                                    <button class="NavigationButton" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers + 1), @(ViewBag.order))">@(ViewBag.PageNumberTransfers + 1)</button>
                                </div>
                                <div class="paginationRightButtondv">
                                    <button class="PaginationButtons" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers+1), @(ViewBag.order))">Next</button>
                                </div>
                            }
                            else
                            {
                                <div class="paginationMiddleButtondv">
                                    <button class="PaginationButtons" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), (ViewBag.PageNumberTransfers-1), @(ViewBag.order))">Previous</button>
                                </div>
                                <div class="NavigatonButtonsDiv">
                                    <button class="NavigationButton" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers - 1), @(ViewBag.order))">@(ViewBag.PageNumberTransfers - 1)</button>
                                </div>
                                <div class="NavigatonButtonsDiv">
                                    <button class="NavigationButton activeBtn" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers), @(ViewBag.order))">@(ViewBag.PageNumberTransfers)</button>
                                </div>
                                <div class="NavigatonButtonsDiv">
                                    <button class="NavigationButton" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers + 1), @(ViewBag.order))">@(ViewBag.PageNumberTransfers + 1)</button>
                                </div>
                                <div class="paginationRightButtondv">
                                    <button class="PaginationButtons" onclick="OpenDetailsByEquipmentStart(@(ViewBag.PageNumberTickets), @(ViewBag.PageNumberTransfers+1), @(ViewBag.order))">Next</button>
                                </div>
                            }
