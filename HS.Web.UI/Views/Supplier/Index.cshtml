
@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>
@{
    string url = Request.Url.AbsolutePath;

    if (url.ToLower().IndexOf("/expense") == 0)
    {
        ViewBag.Title = "Expense";
        Html.RenderAction("Expense", "ExpensePartial");
    }
    if (url.ToLower().IndexOf("/supplier/supplierdetail") > -1)
    {
        ViewBag.Title = "SupplierDetails";
        Html.RenderAction("SupplierDetails", "Supplier", new { id = ViewBag.Id });
    }

}
