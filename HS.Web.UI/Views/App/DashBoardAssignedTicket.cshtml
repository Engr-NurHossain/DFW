                            </div>
                            <div>
                                <div class="dbat_device_header">@Localize.T("Ticket Type") <span>:</span></div>
                                <div class="dbat_device_info"> <span title="@Localize.T("Ticket Type")">@Html.DisplayFor(modelItem => item.TicketType)</span></div>
