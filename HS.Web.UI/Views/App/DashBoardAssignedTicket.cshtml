                                    <a title="@Html.DisplayFor(modelItem => item.CustomerName)" href="@(AppConfig.DomainSitePath)/Customer/Customerdetail/?id=@item.CustomerId" target="" class="cus-anchor">
                                        @Html.DisplayFor(modelItem => item.CustomerName)
                                    </a>
                                </div>
                            </div>
                            <div>
