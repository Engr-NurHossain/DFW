                        <div class="form-group clearfix col-md-3">
                            <input class="form-control EstimateFilterEndDate" placeholder="End Date" id="install_goback_max_date" />
                        </div>
                        <div class="form-group clearfix col-md-3">
                            <input type="text" value="@ViewBag.Price" placeholder="Over X Price" id="overxprice" class="form-control overxprice" />

                        </div>



                        <div class="form-group clearfix col-md-3">
                            <div class="input-group add-on">
                                @Html.DropDownList("Estimatorstatus", (List<SelectListItem>)ViewBag.StatussList, new { @class = "form-control PTOFilter", @placeholder = "select status" })

                                <div class="input-group-btn">
                                    <button class="btn EstimatorFilterBtn green_button" title="@Localize.T("Search")">
                                        <i class="glyphicon glyphicon-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="panel panel-default Estimator-list-panel">

                    </div>
                </div>
            </div>
        </div>
    }

</div>

<script>

    var EstimatorPagination = function (pageno, order) {
        var startdate = $(".EstimateFilterStartDate").val();
        var enddate = $(".EstimateFilterEndDate").val();
        var overxprice = $(".overxprice").val();
        var status = $("#Estimatorstatus").val();



        var TabsLoaderText = "<div class='invoice-loader'><div><div class='lds-css ng-scope'><div style='margin:auto; z-index:99;' class='lds-double-ring'><div></div><div></div></div></div></div></div>";
        $(".Estimator-list-panel").html(TabsLoaderText);
        $(".Estimator-list-panel").load(domainurl + "/App/DashBoardEstimator/?pageno=" + pageno + "&pagesize=20" + "&order=" + order+"&startdate="+startdate+"&enddate="+enddate+"&overxprice="+overxprice+"&status="+status);
    }
    var AssignedTicketPaging= function (pageno, order) {
       if ($("#ticket_schedule_max_date").val() == "") {
                 $("#ticket_schedule_max_date").val("alltime");
             }
        if ($("#ticket_schedule_min_date").val() == "") {
                 $("#ticket_schedule_min_date").val("alltime");
        }
        AssignedTicketPagination(pageno, order);
    }
       var AssignedTicketPagination = function (pageno, order) {
        var startdate = $(".ticket_schedule_min_date").val();
        var enddate = $(".ticket_schedule_max_date").val();
           var TabsLoaderText = "<div class='invoice-loader'><div><div class='lds-css ng-scope'><div style='margin:auto; z-index:99;' class='lds-double-ring'><div></div><div></div></div></div></div></div>";
           console.log(startdate + "assigned tickeyy");
           console.log(enddate + "assigned tickeyy");
           //get tommorow date and last -6 day
           var nextdayfull = new Date(new Date().getTime() + 24 * 60 * 60 * 1000);
           var day = nextdayfull.getDate()
           var month = nextdayfull.getMonth() + 1
           var year = nextdayfull.getFullYear()
           var nextday = (month + "/" + day + "/" + year);
           var prevdatefull = new Date(new Date().getTime() - 6*(24 * 60 * 60 * 1000));
           var dayprev = prevdatefull.getDate()
           var monthprev = prevdatefull.getMonth() + 1
           var yearprev = prevdatefull.getFullYear()
           var previousday = (monthprev + "/" + dayprev + "/" + yearprev);
           //end
           if (startdate == "") {
               startdate = previousday;
               $(".ticket_schedule_min_date").val(previousday);
           }
           if (enddate == "") {
               enddate = nextday;
               $(".ticket_schedule_max_date").val(nextday);
           }
            if (startdate == "alltime") {
               startdate = "";
               $(".ticket_schedule_min_date").val(startdate);
           }
           if (enddate == "alltime") {
               enddate = "";
               $(".ticket_schedule_max_date").val(enddate);
           }


        $(".Assigned-Ticket-list-panel").html(TabsLoaderText);
        $(".Assigned-Ticket-list-panel").load(domainurl + "/App/DashBoardAllTicket/?pageno=" + pageno + "&pagesize=20" + "&order=" + order+"&ScheduleMinDate="+startdate+"&ScheduleMaxDate="+enddate);
    }

    $(document).ready(function () {
        if ($(".estimator_list_style .col-lg-6").length == 1) {
            $(".estimator_list_style div:first(1)").removeClass("col-lg-6");
            $(".estimator_list_style div:first(1)").addClass("col-lg-12");
        }
        @if (PermissionChecker.IsPermitted(PermissionList.Dashboard.UpCommingBirthDays))
        {
             <text>$(".Birthday-list-panel").load(domainurl + "/App/EmployeeBirthdayList");</text>
        }
        @if (PermissionChecker.IsPermitted(PermissionList.Dashboard.UpcomingAnniversaries))
        {
             <text>$(".Anniversary-list-panel").load(domainurl + "/App/EmployeeAnniversaryList");</text>
        }
        @if (PermissionChecker.IsPermitted(PermissionList.Dashboard.GeeseActivityLog))
        {
             <text>$(".GeeseActivityLog-list-panel").load(domainurl + "/App/GeeseActivityLog");</text>
        }
        @if (PermissionChecker.IsPermitted(PermissionList.Dashboard.AssignedTickets))
        {
            <text>$(".Ticket-list-panel").load(domainurl + "/App/DashBoardAssignedTicket");</text>
        }

        @if (PermissionChecker.IsPermitted(PermissionList.Dashboard.Reminders))
        {
            <text>$(".reminder-followup-div").load(domainurl + "/App/DashBoardFollowupReminder");</text>
        }
        @if (PermissionChecker.IsPermitted(PermissionList.Dashboard.CurrentUserReminders))
        {
            <text>$(".currentuserreminder-followup-div").load(domainurl + "/App/DashBoardCurrentUserFollowupReminder");</text>
        }
        @if (PermissionChecker.IsPermitted(PermissionList.Dashboard.EstimatorList))
        {
            <text>EstimatorPagination(1);</text>
        }
          @if (PermissionChecker.IsPermitted(PermissionList.Dashboard.ShowAllTicketInDashboard))
        {
               <text>AssignedTicketPagination(1);</text>

        }
        $("#dashboardReport").load("/app/DashboardInsideSalesReport");
    });
</script>

