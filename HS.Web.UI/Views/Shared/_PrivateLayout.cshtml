    $(document).ready(function () {

        GetMaxBuildVersion();

        if ($('.orglistLayout').length > 0) {
            console.log("layout");
            select2loadprivate();
            loadorglistselect2();
        }


        if (isorderpermit == "True") {
            NewOrderNotificationCount();
        setInterval(function () {
            NewOrderNotificationCount();
        }, 10000);
        }

        $(".PrivateLayoutContents").height(window.innerHeight);
        if (typeof (isieatery) != "undefined" && isieatery != null && isieatery != "" && isieatery != "-1") {
            $(".ChooseCompanyLi").attr("style", "width: 300px;");
            CheckRestaurantSetup();
        }


        $("#subelement-watermark-Files").click(function () {
         
            console.log("clicked");
            OpenRightToLeftModal("@(AppConfig.DomainSitePath)/File/addWMFile");

        })
        $("#LoadTagManager").click(function () {
            LoadTagManager(false);
        });

    });
    $(window).resize(function () {
        setTimeout(function () {
            $(".PrivateLayoutContents").height(window.innerHeight);
        }, 100);

    });
</script>
<nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
    <div class="navbar-header Top-Logo">
        @*logo hide class "Top-Logo"*@

        <a class="navbar-brand" href="@(DomainURL)/Dashboard"><img class="company-white-logo" src="@ViewBag.UserCompanyLogo" loading="lazy"/></a>
    </div>
    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">@Localize.T("Toggle navigation")</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
    </button>
    @if (ViewBag.UserCompanyListLayout != null)
    {
        var userCompanyList = ((List<SelectListItem>)ViewBag.UserCompanyListLayout).OrderBy(ss => ss.Text).ToList();
        <ul class="nav navbar-nav navbar-left navbar-top-links ChooseCompanyUl">
            <li class="ChooseCompanyLi">
                @if (((List<SelectListItem>)userCompanyList).Count > 1)
                {
                    @Html.DropDownList("UserCompanyListLayout", (List<SelectListItem>)userCompanyList, new { @class = "form-control orglistLayout" })
                }
                else
                {
                    <label title="@(((List<SelectListItem>)userCompanyList).FirstOrDefault().Text)">@(((List<SelectListItem>)userCompanyList).FirstOrDefault().Text)</label>
                    <input type="hidden" value="@(((List<SelectListItem>)userCompanyList).FirstOrDefault().Value)" id="UserCompanyListLayout" />
                }

            </li>
        </ul>
    }
<ul class="nav navbar-right navbar-top-links alarm right_side_menu_style layout_top_menu_bar">

    @if (PermissionChecker.IsPermitted(PermissionList.MenuPermissions.TopMenuSetting))
