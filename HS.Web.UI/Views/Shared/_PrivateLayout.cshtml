@using Localize = HS.Web.UI.Helper.LanguageHelper
@using PermissionList = HS.Framework.UserPermissions;
@using PermissionChecker = HS.Web.UI.Helper.PermissionHelper;
@using AppConfig = HS.Framework.Utils.AppConfig;
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var currentLoggedIn = ((HS.Web.UI.Helper.CustomPrincipal)(User));
    string DomainURL = AppConfig.DomainSitePath;


    HS.Entities.Seo seo = new HS.Facade.UserInitializer().GetSeoDataByPageUrl(Request.Url.AbsolutePath);
}
@section metainfo{
    <title>@(seo != null ? seo.MetaTitle : "RMR Cloud || Admin panel")</title>
    @RenderSection("PrivateMetaInfo", required: false)
}
@section styles{
    @Styles.Render("~/styles/privatelayout")
    @RenderSection("PrivateStyles", required: false)
    <link href="~/Content/JQueryFileUpload/jquery.fileupload.css" rel="stylesheet" />
    <link href="~/Content/Select2/select2.css" rel="stylesheet" />
}
@section scripts{
    <script src="~/Content/Js/Layout/Device.js"></script>
    @Scripts.Render("~/scripts/privatelayout")
    @RenderSection("PrivateScripts", required: false)
}
<style>
    .highlight {
        background: yellow;
        font-weight: bold;
    }
    .cls_autofill{
        background-color:#fff !important;
    }
    .buildversion {
        margin-top: 5px;
        font-size: 15px;
        font-weight: normal;
    }
</style>

<script type="text/javascript">

    var TransCurrency = '@HS.Web.UI.Helper.LabelHelper.CurrentTransMakeCurrency.MakeCurrency(null)';
    var SiteUrl = '@HS.Framework.Utils.AppConfig.SiteDomain';
    var DomainSitePath = '@(DomainURL)';
    var UserCurrentCompanyId = '@ViewBag.DefaultCompanyIdLayout';
    var CurrentUserFullName = '@currentLoggedIn.GetFullName()';
    var CurrentUserUserId  = '@currentLoggedIn.UserId'
    var TabPopStateCheck = false;
    var LoadMyTickets = function (reload) {
        var loadurl = domainurl + "/Ticket/LoadMyTickets";
        var seturl = domainurl + "/MyTicket";
        LoadUrlContents(loadurl, seturl, reload);
    }
    var searchHighlight = function (searchText) {
        console.log(searchText);
        if (searchText != null) {
            var Content = $(".GlobalSearchResults").html(); //.replace(/\s+/g, " ")
            console.log(Content);
            var searchExp = new RegExp(searchText, "ig");
            console.log(searchExp);
            var matches = Content.match(searchExp);
            if (matches) {
                $(".GlobalSearchResults").html(Content.replace(searchExp, function (match) { //textContent.
                    return "<span class='highlight'>" + match + "</span>"
                }));
            }
            else {
                $(".highlight").removeClass("highlight");
            }
        }
        else {
            $(".highlight").removeClass("highlight");
        }
    };
    $(document).ready(function () {

        /*start input box autofill*/
        $("input").each(function () {
                if ($(this).attr("readonly") != "readonly") {
                    $(this).attr("readonly", "readonly");
                    $(this).addClass("cls_autofill");
                }
            })
        $("input").on("focus", function () {
            if ($(this).hasClass("cls_autofill")) {
                if (this.hasAttribute('readonly')) {
                this.removeAttribute('readonly');
                this.blur();
                this.focus();
            }
            }
        })
        /*end input box autofill*/
        if (window.innerWidth < 769) {
            $(".dropdown-user").width(window.innerWidth - 40);
        }
        else {

        }
        if (window.innerWidth < 415) {
            $(".dropdown_notification").width(window.innerWidth);
            $(".dropdown_help").width(window.innerWidth);
        }
        else {

        }
        //$("#GlobalSearchButton").blur(function () {
        ////$(".GlobalSearchInp").search(function () {    
        //    searchHighlight($(".GlobalSearchInp").val());
        //    console.log($(".GlobalSearchResults").text());
        //});
        $(window).on('popstate', function (item) {
            if (top.location.hash != '') {
                if ($("[tabname='" + top.location.hash + "']").length > 0) {
                    TabPopStateCheck = true;
                    //Excluded some Inventory related tabs in this, as this code was sending request 2-3 times to the server which was increasing the response time and
                    // loading the Partial Views for inventory module 2-3 times, results heavy loading on UI.
                    switch (top.location.hash) {
                        case '#InventoryTab': //skip Inventory Tab click
                            break;
                        case '#PurchaseOrderTab': //skip Purchase Order Tab click
                            break;
                        case '#TechReceiveTab': //skip Transfer Tab click
                            break;
                        case '#TransferLogTab': //skip Transfer Log Tab click
                            break;
                        default:
                            $("[tabname='" + top.location.hash + "']").click();
                            $("#Right-To-Left-Modal-Body .close").click();
                            $(".modal-backdrop").hide();
                            CloseTopToBottomModal();
                            break;
                    }
                }
                else if ($("[alttabname='" + top.location.hash + "']").length > 0) {
                    if (top.location.hash == "#addContact") {
                        $("[alttabname='" + top.location.hash + "']").click();
                    }
                    else if (top.location.hash == "#addOpportunity") {
                        $("[alttabname='" + top.location.hash + "']").click();
                    }
                    else if (top.location.hash == "#addActivity") {
                        $("[alttabname='" + top.location.hash + "']").click();
                    }
                    else if (top.location.hash == "#addNote") {
                        $("[alttabname='" + top.location.hash + "']").click();
                    }
                    else if (top.location.hash == "#addCorrespondence") {
                        $("[alttabname='" + top.location.hash + "']").click();
                    }
                    else if (top.location.hash == "#addFile") {
                        $("[alttabname='" + top.location.hash + "']").click();
                    }
                }
                else {
                    var windoloc = window.location.href;
                }
            } else {
                window.location = window.location.href;
            }
        });
        //$('[data-toggle="tooltip"]').tooltip();
    });
    $(document).bind('keydown', function (e) {
        if (e.keyCode == 13) {
            $('#GlobalSearchButton').trigger('click');
        }
    });
    $(window).resize(function () {
        if (window.innerWidth < 768) {
            $(".dropdown-user").width(window.innerWidth);
        }
        else {

        }
        if (window.innerWidth < 415) {
            $(".dropdown_notification").width(window.innerWidth);
            $(".dropdown_help").width(window.innerWidth);
        }
        else {

        }
    })
</script>
<script>
    var comid = '@currentLoggedIn.CompanyId.Value';
    var isieatery = '@Request.Url.Host.ToLower().IndexOf("app.ieatery.com")';
    var isorderpermit = '@PermissionChecker.IsPermitted(PermissionList.MenuPermissions.LeftMenuOrders)';
    var NewOrderNotificationCount = function () {
        var StartDate = $(".min-date").val();
        var EndDate = $(".max-date").val();
        var url = "/App/NewOrderNotificationCount";
        var param = JSON.stringify({
            companyid: comid,
            startdate: StartDate,
            enddate: EndDate
        })
        $.ajax({
        type: "POST",
            url: url,
            data: param,
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        cache: false,
            success: function (data) {
                if (data.result && parseInt(data.neworder) > 0) {
                    $("#LoadOrders1 i.order_notification_icon").attr("style", "display:block;");
                    $("#LoadOrders1 i.order_notification_icon").html("<span class='order_notification_count'>" + data.neworder + "</span>");
                }
                else {
                    $("#LoadOrders1 i.order_notification_icon").attr("style", "display:none;");
                }
        },
        error: function (jqXHR, textStatus, errorThrown) {
            $(".loader-div").hide();
            console.log(errorThrown);
        }
    });
    }
    var CheckRestaurantSetup = function () {
        var url = "/App/CheckRestaurantSetup";
        var param = JSON.stringify({
            companyid: comid
        })
        $.ajax({
        type: "POST",
            url: url,
            data: param,
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        cache: false,
            success: function (data) {
                if (data.result == true) {
                    $(".setup_setupConfigUI").addClass("hidden");
                    $(".web_setupConfigUI").removeClass("hidden");
                    $(".web_setupConfigUI li button").attr("onclick", "window.open('https://www.ieatery.com" + data.weburl + "', '_blank')")
                }
                else {
                    $(".setup_setupConfigUI").removeClass("hidden");
                    $(".web_setupConfigUI").addClass("hidden");
                }
                if (data.isitem == true) {
                    $(".no_item_found").addClass("hidden");
                }
                else {
                    $(".no_item_found").removeClass("hidden");
                }
        },
        error: function (jqXHR, textStatus, errorThrown) {
            $(".loader-div").hide();
            console.log(errorThrown);
        }
    });
    }
    var GetMaxBuildVersion = function () {
        var url = "/App/GetMaxBuildVersion"; 
        $.ajax({
            type: "POST",
            url: url, 
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            data: {},
            cache: false,
            success: function (data) { 
                if (data.result == true) {
                    var version = data.maxversion; 
                    var formattedVersion = version;//.toFixed(1); 
                    $("#spanbuildversion").text(formattedVersion);
                    if (data.maxbuilddate != null)
                    {
                        var buildDate = new Date(parseInt(data.maxbuilddate.replace(/[^0-9]/g, '')));
                        var formattedDate = buildDate.toLocaleDateString();
                        $("#spanbuilddate").text(formattedDate); 
                    }
                    /*window.location.reload();*/
                } 
            },
            error: function (jqXHR, textStatus, errorThrown) {
                $(".loader-div").hide();
                console.log(errorThrown);
            }
        });
    }
    var loadorglistselect2 = function () {
        $('.orglistLayout').select2();
    }
