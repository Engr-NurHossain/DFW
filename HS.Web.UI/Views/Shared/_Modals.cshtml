            <div class="modal-content">

                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                </div>

                <div class="modal-body">

                </div>

            </div>
        </div>
    </div>

    <div class="modal right fade" id="Right-To-Left-Modal-Body-MMR" style="z-index:999999!important;" role="dialog" aria-labelledby="myModalLabel2">
        <div class="modal-dialog" role="document">
            <div class="modal-content">

                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                </div>

                <div class="modal-body">

                </div>

            </div>
        </div>
    </div>

    <div class="modal right fade" id="Right-To-Left-big-Modal-Body" role="dialog" aria-labelledby="myModalLabel2">
        <div class="modal-dialog" id="modal-dialog-lg" role="document">
            <div class="modal-content">

                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                </div>

                <div class="modal-body">

                </div>

            </div>
        </div>
    </div>

    <div class="modal fade" id="ModalCancelCustomer" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header" style="width:100%">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title cancel-title">Rolled over Customer!</h4>
                </div>
                <br />
                <br />
                <div class="others-body cancel-body">
                    <div class="cus-body" style="padding-top: 15px;">
                        <p>Reason @Html.TextArea("CustomerCancel", new { @class = "form-control CustomerCancel" })</p>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" id="btnCancelCustomer" onclick="DefaultCancelCustomerfunc(true);" data-dismiss="modal">Submit</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="ModalOpenCreditAndAmmount" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header message_header_container">
                    <button type="button" class="closenew" data-dismiss="modal">&times;</button>
                    <div class="message_header_border"></div>
                    <span class="modal-title message_header_title_credit"></span>
                </div>
                <div class="success-body message_content_div">
                    <div class="cus-body">
                        <p></p>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" id="__ConfirmationMessageModal_" onclick="DefaultSuccessfunc(true);" data-dismiss="modal">Submit</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="ModalSuccessIeateryPopup" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header message_header_container">
                    <span class="modal-title message_header_title">Company Change verification.!</span>
                </div>
                <div class="message_middle_icon_div">
                    <div class="message_middle_icon"><i class="fa fa-check"></i></div>
                </div>
                <div class="others-body message_content_div">
                    <div class="cus-body">
                        <p>ModalSuccessMessage.</p>
                    </div>
                </div>
                <div class="modal-footer">
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="ModalLeaveMessage" style="z-index:99999999!important;" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header message_header_container">
                    <button type="button" class="closenew" data-dismiss="modal">&times;</button>
                    <div class="message_header_border"></div>
                    <span class="modal-title message_header_title">Company Change verification.!</span>
                </div>
                <div class="message_middle_icon_div">
                    <div class="message_middle_icon"><i class="fa fa-warning"></i></div>
                </div>
                <div class="others-body message_content_div">
                    <div class="cus-body">
                        <p>ModalSuccessMessage.</p>
                    </div>
                    <div class="cus-body conditional_msg">
                        <p>ModalSuccessMessage.</p>
                    </div>

                </div>
                <div class="modal-footer success-footer">
                    <button type="button" class="btn btn-no red_button" onclick="DefaultTaskAssignRejectfunc()" data-dismiss="modal">No</button>
                    <button type="button" class="btn btn-green green_button" onclick="DefaultTaskAssignConfirmationfunc()" data-dismiss="modal" id="btn_yes">Yes</button>
                </div>
                @*<div class="modal-footer">
                        <div class="error-foot">
                            <button type="button" class="btn btn-default" onclick="DefaultErrorfunc();" data-dismiss="modal">Close</button>
                        </div>
                    </div>*@
