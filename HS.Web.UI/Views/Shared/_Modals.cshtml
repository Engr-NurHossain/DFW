@{
    Layout = null;
}
@Styles.Render("~/styles/Modals")

<div class="ModalListDiv">
    <div style="display:none">
        <button type="button" id="RightToLeftModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#Right-To-Left-Modal-Body">Open Modal</button>
        <button type="button" id="RightToLeftModal1" class="btn btn-info btn-lg" data-toggle="modal" data-target="#Right-To-Left-Modal-Body1">Open Modal</button>
        <button type="button" id="RightToLeftBigModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#Right-To-Left-big-Modal-Body">Open Modal</button>
        <button type="button" id="RightToLeftModalMMR" class="btn btn-info btn-lg" data-toggle="modal" data-target="#Right-To-Left-Modal-Body-MMR">Open Modal</button>
        <button type="button" id="ConfirmationMessageModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#ModalConfirmationMessage">Open Modal</button>

        <button type="button" id="SuccessMessageModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#ModalSuccessMessage">Open Modal</button>
        <button type="button" id="OpenTextModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#ModalOpenText">Open Modal</button>

        <button type="button" id="ErrorMessageModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#ModalErrorMessage">Open Modal</button>
        <button type="button" id="CautionMessageModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#ModalCautionMessage">Open Modal</button>
        <button type="button" id="CancelCustomerModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#ModalCancelCustomer">Open Modal</button>
        <button type="button" id="OpenCreditAndAmmountModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#ModalOpenCreditAndAmmount">Open Modal</button>
        <button type="button" id="IeateryPopupModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#ModalSuccessIeateryPopup">Open Modal</button>
        <button type="button" id="LeaveMessageModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#ModalLeaveMessage">Open Modal</button>
        @*<button type="button" id="RMRCustomModalConfirmationModal" class="btn btn-info btn-lg" data-toggle="modal" data-target="#RMRCustomModalConfirmationMessage">Open Modal</button>*@
    </div>
    <div class="modal fade" id="ModalSuccessMessage" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header message_header_container">
                    <button type="button" class="closenew" data-dismiss="modal" onclick="DefaultSuccessfunc(true);">&times;</button>
                    <div class="message_header_border"></div>
                    <span class="modal-title message_header_title">Company Change verification.!</span>
                </div>
                <div class="message_middle_icon_div">
                    <div class="message_middle_icon"><i class="fa fa-check"></i></div>
                </div>
                <div class="others-body message_content_div">
                    <div class="cus-body">
                        <p>ModalSuccessMessage.</p>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default success_close" id="__ConfirmationMessageModal_" onclick="DefaultSuccessfunc(true);" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="ModalOpenText" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header message_header_container">
                    <button type="button" class="closenew" data-dismiss="modal" onclick="DefaultSuccessfunc(true);">&times;</button>
                    <div class="message_header_border"></div>
                    <span class="modal-title message_header_title">Company Change verification.!</span>
                </div>
                <div class="message_middle_icon_div">
                    <div class="message_middle_icon"><i class="fa fa-check"></i></div>
                </div>
                <div class="others-body message_content_div">
                    <div class="cus-body">
                        <p>ModalSuccessMessage.</p>
                    </div>
                </div>
                <div class="others-body message_content_div">
                    <div class="cus-body">
                        <input type="password" id="CustomerOTP" />
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" id="__ConfirmationMessageModal_" onclick="DefaultSuccessfunc(true);" data-dismiss="modal">Ok</button>
                </div>
            </div>
        </div>
    </div>







    <div class="modal fade" id="ModalErrorMessage" style="z-index:99999999!important;" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header message_header_container">
                    <button type="button" class="closenew" data-dismiss="modal" onclick="DefaultErrorfunc();">&times;</button>
                    <div class="message_header_border"></div>
                    <span class="modal-title message_header_title">Company Change verification.!</span>
                </div>
                <div class="message_middle_icon_div">
                    <div class="message_middle_icon"><i class="fa fa-times"></i></div>
                </div>
                <div class="others-body message_content_div">
                    <div class="cus-body">
                        <p class="error-body">ModalSuccessMessage.</p>
                    </div>

                </div>
                <div class="modal-footer">
                    <div class="error-foot">
                        <button type="button" class="btn btn-default" onclick="DefaultErrorfunc();" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="ModalCautionMessage" style="z-index:99999999!important;" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header message_header_container">
                    <button type="button" class="closenew" data-dismiss="modal" onclick="DefaultCautionfunc();">&times;</button>
                    <div class="message_header_border"></div>
                    <span class="modal-title message_header_title">Caution!</span>
                </div>
                <div class="message_middle_icon_div">
                    <div class="message_middle_icon"><i class="fa fa-exclamation-triangle"></i></div>
                </div>
                <div class="others-body message_content_div">
                    <div class="cus-body">
                        <p class="error-body">ModalCautionMessage.</p>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="error-foot">
                        <button type="button" class="btn btn-default" onclick="DefaultCautionfunc();" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="TopToBottomModal">
        <div class="modal_transparent_bg"></div>
        <div class="top_to_bottom_modal_container">
            <div class="header-other-right-menu">
                <div onclick="CloseTopToBottomModal()" class="close-div">
                    <img class="close_btn_size" title="Close" src="~/Content/Icons/cross_update.png" />
                </div>
            </div>
            <div class="ContentsDiv">

            </div>
        </div>
    </div>

    <div id="FullScreenLoginModal" class="FullScreenLoginModal">
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-md-offset-4">
                    <div class="login-panel panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title">Please Sign In</h3>
                        </div>
                        <div class="panel-body">
                            <form role="form">
                                <fieldset>
                                    <div class="form-group">
                                        <input class="form-control" autocomplete="off" placeholder="Username" id="UserName" name="email" type="email" autofocus>
                                    </div>
                                    <div class="form-group">
                                        <input class="form-control text-security-disk" autocomplete="off" placeholder="Password" id="Password" name="password" type="password" value="">
                                    </div>
                                    <div class="checkbox">
                                        <label>
                                            <input name="remember" type="checkbox" id="Remember" checked="checked" value="Remember Me">Remember Me
                                        </label>
                                    </div>
                                    <input type="button" id="BtnLogin" class="btn btn-lg btn-success btn-block" value="Log in" />
                                    <span>
                                        Forgot your password? Click <a class="cus-anchor" href="@(HS.Framework.Utils.AppConfig.DomainSitePath)/Login/ForgotPassword">here</a>.
                                    </span>
                                </fieldset>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @*
        *****Keep In Mind***
        1. Tiny MCE Z-Index 65537
