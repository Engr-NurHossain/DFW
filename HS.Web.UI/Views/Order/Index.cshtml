
@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>

@using AppConfig = HS.Framework.Utils.AppConfig;

@{
    string url = Request.Url.AbsolutePath.ToLower();
    string DomainSitePath = AppConfig.DomainSitePath;
    if (url.IndexOf(DomainSitePath + "/order/orderdetail") > -1)
    {
        ViewBag.Title = "OrderDetails";
        Html.RenderAction("DetailTab", "Order", new { id = ViewBag.Id });
    }
    else if (url.IndexOf(DomainSitePath + "/review") == 0)
    {
        ViewBag.Title = "Review";
        Html.RenderAction("ReviewPartial", "Order", new { id = ViewBag.Id });
    }
    else if (url.IndexOf(DomainSitePath + "/reward") == 0)
    {
        ViewBag.Title = "Reward";
        Html.RenderAction("RewardPartial", "Order", new { id = ViewBag.Id });
    }
    else if (url.IndexOf(DomainSitePath + "/order") == 0)
    {
        ViewBag.Title = "Order";
        Html.RenderAction("Order", "Order");
    }
    else if (url.IndexOf(DomainSitePath + "/loadorderreports") == 0)
    {
        ViewBag.Title = "Order Report";
        Html.RenderAction("OrderReports", "Order");
    }
}
