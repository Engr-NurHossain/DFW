
@{
    Layout = "~/Views/Shared/_PrivateLayout.cshtml";
}
<script>
    $(document).ready(function () {
        $(".LoaderWorkingDiv").hide();
    })
</script>

@{ 

    string url = Request.Url.AbsolutePath.ToLower();
    if (url.IndexOf("/citytaxinfo") == 0)
    {
        ViewBag.Title = "City Tax Info";
        Html.RenderAction("CityTaxPartial", "CityTax");
    }

    
   else if (url.ToLower().IndexOf("/restrictedzipcode") == 0)
    {
        ViewBag.Title = "Restricted zipcode";
        Html.RenderAction("RestrictedZipCode", "CityTax");
    }
}