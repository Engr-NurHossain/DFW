@using Localize = HS.Web.UI.Helper.LanguageHelper
@{
    Layout = null;
}
@using AppConfig = HS.Framework.Utils.AppConfig; 
<link href="~/Content/Css/AddIncome/AddIncome.css" rel="stylesheet" />
<script src="~/Content/Js/AddIncome/AddIncome.js"></script>
<div class="page-wrap">
    <div class="container-fluid">
        <div class="AddIncome-modal-content">
            <h3 class="txt-design">@Localize.T("Add Income Information")</h3>
        </div>
    </div>
    <div class="form-group clearfix">
        <div class="row">
            <div class="col-md-6">
                <span>@Localize.T("Income Amount")</span>
            </div>
            <div class="col-md-6">

            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <span>@Localize.T("Payment Status")</span>
            </div>
            <div class="col-md-6">

            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <span>@Localize.T("Payment Method")</span>
            </div>
            <div class="col-md-6">

            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <span>@Localize.T("Income Date")</span>
            </div>
            <div class="col-md-6">

            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <span>@Localize.T("Notes")</span>
                @*@Html.TextAreaFor(new { @class = "form-control", id = "Notes" })*@
            </div>
            
        </div>
    </div>
</div>